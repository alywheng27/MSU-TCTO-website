---
import { urlForImage } from '../api/api';

import { get3LatestArticle } from '../api/api';
const threeLatestArticles = await get3LatestArticle();
---






<div class="bg-msu-deep-ocean text-white-pure">
    <!-- Latest Articles Section -->
    <div class="bg-gradient-to-r from-msu-deep-ocean to-msu-deep-ocean py-16">
        <div class="container mx-auto px-4 xl:px-[12.5%]">
            <h2 class="text-3xl md:text-4xl font-bold mb-8 text-center">
                <span class="bg-clip-text text-transparent bg-gradient-to-r from-white to-white">
                    Latest University News
                </span>
            </h2>
            <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
                {threeLatestArticles.map((article) => (
                    <div class="group relative overflow-hidden rounded-xl shadow-lg transition-all duration-500 hover:shadow-xl hover:-translate-y-2">
                        <div class="relative h-48 overflow-hidden">
                            <img 
                                src={urlForImage(article.mainImage.asset.url).url()} 
                                alt={article.title} 
                                class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-105"
                            />
                            <div class="absolute inset-0 bg-gradient-to-t from-black/80 to-transparent"></div>
                        </div>
                        <div class="bg-white/5 backdrop-blur-sm p-6">
                            <h3 class="text-xl font-bold mb-3 line-clamp-2">
                                <a href={`/${article.slug.current}`} class="hover:text-msu-gold transition-colors">
                                    {article.title}
                                </a>
                            </h3>
                            <p class="text-titanium line-clamp-3 mb-4">
                                {article.body[0].children[0].text}
                            </p>
                            <a href={`/${article.slug.current}`} class="inline-flex items-center text-white font-medium group">
                                Read More
                                <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 ml-1 transition-transform group-hover:translate-x-1" viewBox="0 0 20 20" fill="currentColor">
                                    <path fill-rule="evenodd" d="M10.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L12.586 11H5a1 1 0 110-2h7.586l-2.293-2.293a1 1 0 010-1.414z" clip-rule="evenodd" />
                                </svg>
                            </a>
                        </div>
                    </div>
                ))}
            </div>
            <div class="text-center mt-10">
                <a href="/publications/articles" class="inline-block bg-gradient-to-r from-msu-bgc-color to-msu-bgc-color hover:from-msu-main-color hover:to-msu-deep-ocean text-msu-deep-ocean font-semibold py-3 px-8 rounded-full transition-all duration-300 hover:shadow-lg hover:shadow-msu-main-color/30">
                    View All News Articles
                </a>
            </div>
        </div>
    </div>

            <!-- partners removed -->

    <!-- Quick Links Section -->
