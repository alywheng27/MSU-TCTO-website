	<script>
		// Simplified Conference Audio System - Motivational Music Only
		class ConferenceAudio {
			constructor() {
				this.sounds = {};
				this.isEnabled = true;
				this.volume = 0.4;
				this.initializeSounds();
			}

			initializeSounds() {
				// Try to load motivational music, fallback to existing audio
				this.loadMotivationalMusic();
			}

					loadMotivationalMusic() {
			console.log('Loading motivational music...');
			// Primary: Look for user's motivational.mp3
			const motivationalAudio = this.createAudio('/audio/motivational.mp3');
			
			if (motivationalAudio) {
				console.log('Motivational.mp3 loaded successfully');
				this.sounds.motivational = motivationalAudio;
				this.setupAudioProperties();
			} else {
				console.log('Motivational.mp3 failed, trying fallback...');
				// Fallback: Use existing himno_tcto.mp3 as background music
				const fallbackAudio = this.createAudio('/audio/himno_tcto.mp3');
				if (fallbackAudio) {
					console.log('Fallback audio loaded successfully');
					this.sounds.motivational = fallbackAudio;
					this.setupAudioProperties();
				} else {
					console.error('No audio could be loaded');
				}
			}
		}

		setupAudioProperties() {
			if (this.sounds.motivational) {
				this.sounds.motivational.loop = true;
				this.sounds.motivational.volume = this.volume;
				this.sounds.motivational.autoplay = true; // Enable autoplay
				console.log('Audio properties set up, volume:', this.volume);
				
				// Handle load events
				this.sounds.motivational.addEventListener('loadeddata', () => {
					console.log('Audio data loaded and ready to play');
					// Try to auto-play when data is loaded
					this.attemptAutoPlay();
				});
				
				this.sounds.motivational.addEventListener('canplay', () => {
					console.log('Audio can start playing');
					// Try to auto-play when ready
					this.attemptAutoPlay();
				});
				
				// Handle load errors gracefully
				this.sounds.motivational.addEventListener('error', (e) => {
					console.warn('Audio file could not be loaded:', e);
				});

				// Add play/pause event listeners for debugging
				this.sounds.motivational.addEventListener('play', () => {
					console.log('Audio started playing');
				});
				
				this.sounds.motivational.addEventListener('pause', () => {
					console.log('Audio paused');
				});
			}
		}

		// Attempt to auto-play music
		attemptAutoPlay() {
			if (this.sounds.motivational && this.isEnabled) {
				this.sounds.motivational.play().then(() => {
					console.log('Autoplay successful');
				}).catch(e => {
					console.warn('Autoplay prevented by browser:', e);
					// If autoplay fails, we'll still allow manual control
				});
			}
		}

			createAudio(dataUrl) {
				try {
					const audio = new Audio(dataUrl);
					audio.volume = this.volume;
					return audio;
				} catch (error) {
					console.warn('Could not create audio:', error);
					return null;
				}
			}

			// Simple placeholder methods (no AI sounds)
			playButtonClick() {
				// No sound - music only
			}

			playSuccess() {
				// No sound - music only
			}

			playPageTransition() {
				// No sound - music only
			}

			playModalOpen() {
				// No sound - music only
			}

			playModalClose() {
				// No sound - music only
			}

					// Start motivational music
		startAmbient() {
			if (!this.isEnabled) return;
			
			if (this.sounds.motivational) {
				this.sounds.motivational.volume = this.volume;
				this.sounds.motivational.play().catch(e => console.warn('Motivational music failed:', e));
			}
		}

		// Stop motivational music
		stopAmbient() {
			if (this.sounds.motivational) {
				this.sounds.motivational.pause();
				this.sounds.motivational.currentTime = 0;
			}
		}

		// Play music
		playMusic() {
			console.log('Attempting to play music...', this.sounds.motivational, this.isEnabled);
			if (this.sounds.motivational && this.isEnabled) {
				this.sounds.motivational.play().catch(e => console.warn('Play failed:', e));
				console.log('Music play command sent');
			}
		}

		// Pause music
		pauseMusic() {
			console.log('Attempting to pause music...', this.sounds.motivational);
			if (this.sounds.motivational) {
				this.sounds.motivational.pause();
				console.log('Music pause command sent');
			}
		}

		// Check if music is playing
		isPlaying() {
			const playing = this.sounds.motivational && !this.sounds.motivational.paused;
			console.log('Music playing status:', playing);
			return playing;
		}

		// Toggle play/pause
		togglePlayPause() {
			console.log('Toggle play/pause called');
			const currentlyPlaying = this.isPlaying();
			console.log('Currently playing:', currentlyPlaying);
			
			if (currentlyPlaying) {
				this.pauseMusic();
				return false; // paused
			} else {
				this.playMusic();
				return true; // playing
			}
		}

		// Toggle audio system (enable/disable)
		toggle() {
			this.isEnabled = !this.isEnabled;
			if (!this.isEnabled) {
				this.stopAmbient();
			} else {
				this.startAmbient();
			}
			return this.isEnabled;
		}

			// Set volume (0.0 to 1.0)
			setVolume(volume) {
				this.volume = Math.max(0, Math.min(1, volume));
				Object.values(this.sounds).forEach(sound => {
					if (sound) sound.volume = this.volume;
				});
			}

			// Removed AI sound methods - using music only
		}

		// Initialize conference audio system
		const conferenceAudio = new ConferenceAudio();
		
		// Make it globally accessible for debugging
		window.conferenceAudio = conferenceAudio;
		
		// Try to start music immediately on page load
		document.addEventListener('DOMContentLoaded', function() {
			console.log('Page loaded, attempting immediate autoplay...');
			setTimeout(() => {
				conferenceAudio.attemptAutoPlay();
			}, 500); // Small delay to ensure audio is loaded
		});

		// Registration Modal Functionality
		document.addEventListener('DOMContentLoaded', function() {
			const registerBtn = document.getElementById('registerBtn');
			const viewProgramBtn = document.getElementById('viewProgramBtn');
			const keynoteBtn = document.getElementById('keynoteBtn');
			const registrationModal = document.getElementById('registrationModal');
			const closeModal = document.getElementById('closeModal');
			const cancelRegistration = document.getElementById('cancelRegistration');
			const registrationForm = document.getElementById('registrationForm');

			// Add ID to keynote speakers section for navigation
			const keynoteSpeakersSection = document.querySelector('section.relative.py-16');
			if (keynoteSpeakersSection && keynoteSpeakersSection.textContent.includes('KEYNOTE')) {
				keynoteSpeakersSection.id = 'keynote-speakers';
			}

			// Open registration modal
			registerBtn?.addEventListener('click', function() {
				registrationModal.classList.remove('hidden');
				registrationModal.classList.add('flex');
				document.body.classList.add('overflow-hidden');
			});

			// Close modal functions
			function closeRegistrationModal() {
				registrationModal.classList.add('hidden');
				registrationModal.classList.remove('flex');
				document.body.classList.remove('overflow-hidden');
			}

			closeModal?.addEventListener('click', closeRegistrationModal);
			cancelRegistration?.addEventListener('click', closeRegistrationModal);

			// Close modal when clicking outside
			registrationModal?.addEventListener('click', function(e) {
				if (e.target === registrationModal) {
					closeRegistrationModal();
				}
			});

			// Smooth scroll to program section
			viewProgramBtn?.addEventListener('click', function() {
				const programSection = document.getElementById('program');
				if (programSection) {
					programSection.scrollIntoView({ 
						behavior: 'smooth',
						block: 'start'
					});
				}
			});

			// Smooth scroll to keynote speakers section
			keynoteBtn?.addEventListener('click', function() {
				const keynoteSpeakersSection = document.getElementById('keynote-speakers');
				if (keynoteSpeakersSection) {
					keynoteSpeakersSection.scrollIntoView({ 
						behavior: 'smooth',
						block: 'start'
					});
				}
			});

			// Handle form submission
			registrationForm?.addEventListener('submit', function(e) {
				e.preventDefault();
				
				// Get form data
				const formData = new FormData(registrationForm);
				const registrationData = {};
				
				// Convert FormData to object
				for (let [key, value] of formData.entries()) {
					if (registrationData[key]) {
						// Handle multiple values (like checkboxes)
						if (Array.isArray(registrationData[key])) {
							registrationData[key].push(value);
						} else {
							registrationData[key] = [registrationData[key], value];
						}
					} else {
						registrationData[key] = value;
					}
				}

				// Show success message
				alert('Thank you for your registration! We will contact you with further details about the conference.');
				
				// Log registration data (in production, this would be sent to a server)
				console.log('Registration Data:', registrationData);
				
				// Close modal and reset form
				closeRegistrationModal();
				registrationForm.reset();
			});

			// Close modal with Escape key
			document.addEventListener('keydown', function(e) {
				if (e.key === 'Escape' && !registrationModal.classList.contains('hidden')) {
					closeRegistrationModal();
				}
			});

			// Fallback: start music on any user interaction if autoplay failed
			let hasInteracted = false;
			function forcePlayOnInteraction() {
				if (!hasInteracted) {
					hasInteracted = true;
					console.log('User interaction detected, forcing music play...');
					
					// Force play the music
					if (conferenceAudio.sounds.motivational) {
						conferenceAudio.sounds.motivational.play().then(() => {
							console.log('Music started after user interaction');
							updateMusicButtonIcon(true);
						}).catch(e => {
							console.warn('Still failed to play after interaction:', e);
						});
					}
					
					// Remove the listeners after first interaction
					document.removeEventListener('click', forcePlayOnInteraction);
					document.removeEventListener('keydown', forcePlayOnInteraction);
					document.removeEventListener('mousedown', forcePlayOnInteraction);
					document.removeEventListener('touchstart', forcePlayOnInteraction);
				}
			}

			// Add multiple interaction listeners as fallback
			document.addEventListener('click', forcePlayOnInteraction);
			document.addEventListener('keydown', forcePlayOnInteraction);
			document.addEventListener('mousedown', forcePlayOnInteraction);
			document.addEventListener('touchstart', forcePlayOnInteraction);
		});
	</script>

	<!-- Music Control Button -->
	<div id="musicControl" class="fixed top-1/2 right-4 transform -translate-y-1/2 z-40">
		<button 
			id="musicToggle"
			class="bg-gradient-to-r from-msu-deep-ocean to-msu-main-color hover:from-msu-main-color hover:to-msu-deep-ocean text-white p-4 rounded-full shadow-lg hover:shadow-xl transition-all duration-300 transform hover:scale-110 flex items-center justify-center"
			title="Play/Pause Music"
		>
			<svg id="musicIcon" class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
				<!-- Pause icon (default since music auto-plays) -->
				<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 9v6m4-6v6m7-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
			</svg>
		</button>
	</div>

	<script>
		// Music control button functionality
		function updateMusicButtonIcon(isPlaying) {
			const musicIcon = document.getElementById('musicIcon');
			const musicToggle = document.getElementById('musicToggle');
			
			if (isPlaying) {
				// Show pause icon
				musicIcon.innerHTML = '<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 9v6m4-6v6m7-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>';
				musicToggle.title = 'Pause Music';
			} else {
				// Show play icon
				musicIcon.innerHTML = '<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 5v14l11-7z"></path><circle cx="12" cy="12" r="10"></circle>';
				musicToggle.title = 'Play Music';
			}
		}

		document.addEventListener('DOMContentLoaded', function() {
			const musicToggle = document.getElementById('musicToggle');

			// Initialize button state after audio loads
			setTimeout(() => {
				if (conferenceAudio.sounds.motivational) {
					const isPlaying = !conferenceAudio.sounds.motivational.paused;
					updateMusicButtonIcon(isPlaying);
				}
			}, 1000);

			musicToggle?.addEventListener('click', function(e) {
				e.preventDefault();
				e.stopPropagation();
				
				console.log('Music button clicked'); // Debug log
				
				const isPlaying = conferenceAudio.togglePlayPause();
				console.log('Music is now:', isPlaying ? 'playing' : 'paused'); // Debug log
				
				updateMusicButtonIcon(isPlaying);
			});

			// Also ensure the music button is accessible
			window.toggleMusic = function() {
				const isPlaying = conferenceAudio.togglePlayPause();
				updateMusicButtonIcon(isPlaying);
				return isPlaying;
			};
		});
	</script>
</Layout>