---
import Layout from '../layouts/introloader.astro';
import Navbar from "../components/Navbar";
import FooterNav from "../components/FooterNav.astro";
import Footer from "../components/Footer.astro";
import '../styles/css/global.css';
import '../styles/css/globalanimate.css';

const currentPath = Astro.url.pathname;
---

<Layout title="1st International Conference on Inclusive and Indigenous Education | MSU-TCTO">
	<main class="font-sans dark:bg-gray-900 transition-colors duration-300">
		<Navbar path={currentPath} client:visible />

		<!-- Hero Section -->
		<section class="relative bg-msu-deep-ocean min-h-screen flex items-center overflow-hidden pt-[200px]">
			<div class="absolute inset-0 bg-gradient-to-br from-[#61063B]/90 via-[#61063B]/70 to-[#03045e]/90 z-10"></div>
			
			<!-- Parallax Background Image -->
			<div 
				class="absolute inset-0 bg-black/20 z-0"
				style="
					background-image: url('/images/coed/coed.jpg');
					background-size: cover;
					background-position: center;
					background-attachment: fixed;
				"
			></div>

			<!-- Animated Background Elements -->
			<div class="absolute inset-0 z-5">
				<div class="absolute top-1/4 left-1/4 w-64 h-64 bg-msu-gold/10 rounded-full blur-3xl animate-pulse"></div>
				<div class="absolute bottom-1/4 right-1/4 w-96 h-96 bg-msu-main-color/20 rounded-full blur-3xl animate-pulse animation-delay-2000"></div>
				<div class="absolute top-1/2 left-1/2 w-48 h-48 bg-white/5 rounded-full blur-2xl animate-pulse animation-delay-4000"></div>
			</div>
			
			<div class="container mx-auto px-6 z-20 relative">
				<div class="max-w-6xl mx-auto text-center">
					<!-- Conference Badge -->
					<div class="inline-flex items-center bg-white/10 backdrop-blur-md border border-white/20 rounded-full px-6 py-2 mb-8">
						<span class="w-2 h-2 bg-white rounded-full mr-3 animate-pulse"></span>
						<span class="text-white/90 text-sm font-medium">November 26-28, 2025 â€¢ Bongao, Tawi-Tawi</span>
					</div>

					<!-- Main Title -->
					<h1 class="text-3xl md:text-6xl font-bold text-white mb-6 leading-tight">
						<span class="block text-xl md:text-3xl font-normal text-white mb-4">1st International Conference 2025</span>
						<span class="relative">
							International Conference on
							<span class="relative text-white block mt-2" style="text-shadow: 
								-2px -2px 0 #61063B, 
								2px -2px 0 #61063B, 
								-2px  2px 0 #61063B, 
								2px  2px 0 #61063B;">
								Inclusive and Indigenous Education
							</span>
						</span>
					</h1>

					<!-- Subtitle -->
					<p class="text-xl md:text-2xl text-white/90 mb-12 max-w-4xl mx-auto leading-relaxed">
						Fostering social inclusion and providing a dynamic platform for educators, researchers, policymakers, and advocates to share research findings, innovative practices, and critical perspectives on reshaping education systems.
					</p>

					<!-- Call to Action Buttons -->
					<div class="flex flex-wrap justify-center gap-6 mb-16">
						<button 
							id="registerBtn"
							class="group bg-white hover:bg-gray-100 text-msu-deep-ocean font-bold py-4 px-8 rounded-full transition-all duration-300 transform hover:-translate-y-1 shadow-lg hover:shadow-xl flex items-center cursor-pointer"
						>
							<span>Register Now</span>
							<svg class="w-5 h-5 ml-2 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
							</svg>
						</button>
						<button 
							id="viewProgramBtn"
							class="group bg-transparent border-2 border-white hover:bg-white/10 text-white font-bold py-4 px-8 rounded-full transition-all duration-300 transform hover:-translate-y-1 backdrop-blur-md cursor-pointer flex items-center"
						>
							<svg class="w-5 h-5 mr-2 group-hover:rotate-12 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z"></path>
							</svg>
							<span>View Program</span>
						</button>
						<button 
							id="keynoteBtn"
							class="group bg-gradient-to-r from-msu-main-color to-msu-deep-ocean hover:from-msu-deep-ocean hover:to-msu-main-color text-white font-bold py-4 px-8 rounded-full transition-all duration-300 transform hover:-translate-y-1 shadow-lg hover:shadow-xl flex items-center cursor-pointer"
						>
							<span>Meet Our Speakers</span>
							<svg class="w-5 h-5 ml-2 group-hover:translate-y-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3"></path>
							</svg>
						</button>
					</div>

					<!-- Conference Stats -->
					<div class="grid grid-cols-2 md:grid-cols-4 gap-8">
						<div class="text-center">
							<div class="text-3xl md:text-4xl font-bold text-white mb-2">100+</div>
							<div class="text-white/80">Speakers</div>
						</div>
						<div class="text-center">
							<div class="text-3xl md:text-4xl font-bold text-white mb-2">500+</div>
							<div class="text-white/80">Participants</div>
						</div>
						<div class="text-center">
							<div class="text-3xl md:text-4xl font-bold text-white mb-2">50+</div>
							<div class="text-white/80">Countries</div>
						</div>
						<div class="text-center">
							<div class="text-3xl md:text-4xl font-bold text-white mb-2">3</div>
							<div class="text-white/80">Days</div>
						</div>
					</div>
				</div>
			</div>
			
			<!-- Scroll Indicator -->
			<div class="absolute bottom-8 left-1/2 transform -translate-x-1/2 z-20">
				<div class="animate-bounce">
					<svg class="w-6 h-6 text-white/60" fill="none" stroke="currentColor" viewBox="0 0 24 24">
						<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3"></path>
					</svg>
				</div>
			</div>
		</section>

		<!-- Conference Overview Section -->
		<section class="py-20 bg-white dark:bg-gray-900">
			<div class="container mx-auto px-6">
				<div class="max-w-6xl mx-auto">
					<div class="text-center mb-16">
						<h2 class="text-4xl font-bold text-msu-deep-ocean dark:text-white mb-6">Conference Overview</h2>
						<div class="w-24 h-1 bg-msu-deep-ocean dark:bg-yellow-400 mx-auto mb-8"></div>
						<p class="text-xl text-gray-600 dark:text-gray-300 max-w-3xl mx-auto">
							The 1st International Conference on Inclusive and Indigenous Education (ICIIE) aims at fostering social inclusion and providing a dynamic platform to disrupt systemic barriers in education systems.
						</p>
					</div>

					<div class="grid md:grid-cols-3 gap-8 mb-16">
						<!-- Conference Theme -->
						<div class="bg-gradient-to-br from-msu-deep-ocean to-msu-main-color rounded-2xl p-8 text-white transform hover:-translate-y-2 transition-all duration-300 shadow-lg">
							<div class="w-16 h-16 bg-white/20 rounded-full flex items-center justify-center mb-6">
								<svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"></path>
								</svg>
							</div>
							<h3 class="text-2xl font-bold mb-4">Inclusive Education</h3>
							<p class="text-white/90">Reshaping education systems to disrupt systemic barriers and promote social inclusion for all learners.</p>
						</div>

						<!-- Global Collaboration -->
						<div class="bg-gradient-to-br from-white to-gray-100 dark:from-gray-800 dark:to-gray-700 border-2 border-msu-deep-ocean dark:border-yellow-400 rounded-2xl p-8 text-msu-deep-ocean dark:text-white transform hover:-translate-y-2 transition-all duration-300 shadow-lg">
							<div class="w-16 h-16 bg-msu-deep-ocean/20 dark:bg-yellow-400/20 rounded-full flex items-center justify-center mb-6">
								<svg class="w-8 h-8 text-msu-deep-ocean dark:text-yellow-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
								</svg>
							</div>
							<h3 class="text-2xl font-bold mb-4">Indigenous Knowledge</h3>
							<p class="text-msu-deep-ocean/90 dark:text-gray-300">Honoring and integrating indigenous wisdom and perspectives in modern educational frameworks.</p>
						</div>

						<!-- Future-Ready Skills -->
						<div class="bg-gradient-to-br from-msu-main-color to-msu-shallow-ocean rounded-2xl p-8 text-white transform hover:-translate-y-2 transition-all duration-300 shadow-lg">
							<div class="w-16 h-16 bg-white/20 rounded-full flex items-center justify-center mb-6">
								<svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
									<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
								</svg>
							</div>
							<h3 class="text-2xl font-bold mb-4">Research & Innovation</h3>
							<p class="text-white/90">Sharing research findings and innovative practices to advance inclusive and indigenous education approaches.</p>
						</div>
					</div>

					<!-- Key Features -->
					<div class="bg-gray-50 dark:bg-gray-800 rounded-3xl p-8 md:p-12">
						<h3 class="text-3xl font-bold text-msu-deep-ocean dark:text-white text-center mb-12">What Makes This Conference Special</h3>
						<div class="grid md:grid-cols-2 lg:grid-cols-4 gap-8">
							<div class="text-center">
								<div class="w-20 h-20 bg-msu-deep-ocean dark:bg-yellow-400 rounded-full flex items-center justify-center mx-auto mb-4">
									<svg class="w-10 h-10 text-white dark:text-gray-900" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path>
									</svg>
								</div>
								<h4 class="text-lg font-bold text-msu-deep-ocean dark:text-white mb-2">Expert Speakers</h4>
								<p class="text-gray-600 dark:text-gray-300">Renowned educators and researchers from leading universities worldwide</p>
							</div>
							<div class="text-center">
								<div class="w-20 h-20 bg-msu-main-color rounded-full flex items-center justify-center mx-auto mb-4">
									<svg class="w-10 h-10 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"></path>
									</svg>
								</div>
								<h4 class="text-lg font-bold text-msu-deep-ocean dark:text-white mb-2">Interactive Workshops</h4>
								<p class="text-gray-600 dark:text-gray-300">Hands-on sessions to learn practical tools and methodologies</p>
							</div>
							<div class="text-center">
								<div class="w-20 h-20 bg-msu-deep-ocean rounded-full flex items-center justify-center mx-auto mb-4">
									<svg class="w-10 h-10 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 12a9 9 0 01-9 9m9-9a9 9 0 00-9-9m9 9H3m9 9v-9m0-9v9"></path>
									</svg>
								</div>
								<h4 class="text-lg font-bold text-msu-deep-ocean dark:text-white mb-2">Global Networking</h4>
								<p class="text-gray-600 dark:text-gray-300">Connect with educators from over 50 countries</p>
							</div>
							<div class="text-center">
								<div class="w-20 h-20 bg-msu-main-color rounded-full flex items-center justify-center mx-auto mb-4">
									<svg class="w-10 h-10 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
										<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
									</svg>
								</div>
								<h4 class="text-lg font-bold text-msu-deep-ocean dark:text-white mb-2">Research Papers</h4>
								<p class="text-gray-600 dark:text-gray-300">Present and publish your latest educational research</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<!-- World-Class Keynote Speakers Section -->
		<section class="relative py-16 sm:py-20 md:py-24 lg:py-32 bg-gradient-to-br from-slate-50 via-white to-slate-100 dark:from-gray-950 dark:via-gray-900 dark:to-gray-800 overflow-hidden">
			<!-- Advanced Background Elements -->
			<div class="absolute inset-0">
				<!-- Animated Mesh Gradient - Responsive Sizes -->
				<div class="absolute inset-0 opacity-20 sm:opacity-25 md:opacity-30 dark:opacity-15 dark:sm:opacity-20">
					<div class="absolute top-0 left-0 w-[400px] h-[400px] sm:w-[600px] sm:h-[600px] lg:w-[800px] lg:h-[800px] bg-gradient-to-br from-msu-deep-ocean/30 via-transparent to-msu-main-color/20 rounded-full blur-2xl sm:blur-3xl animate-float"></div>
					<div class="absolute bottom-0 right-0 w-[300px] h-[300px] sm:w-[500px] sm:h-[500px] lg:w-[600px] lg:h-[600px] bg-gradient-to-tl from-msu-main-color/25 via-transparent to-msu-deep-ocean/15 rounded-full blur-2xl sm:blur-3xl animate-float animation-delay-2000"></div>
					<div class="absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 w-[500px] h-[200px] sm:w-[700px] sm:h-[300px] lg:w-[1000px] lg:h-[400px] bg-gradient-to-r from-msu-deep-ocean/10 via-msu-main-color/15 to-msu-deep-ocean/10 rounded-full blur-2xl sm:blur-3xl animate-float animation-delay-4000"></div>
				</div>
				
				<!-- Dynamic Particle Effects - Responsive Positioning -->
				<div class="absolute inset-0 opacity-5 sm:opacity-8 md:opacity-10 dark:opacity-3 dark:sm:opacity-5">
					<div class="absolute top-10 left-5 sm:top-20 sm:left-20 w-1.5 h-1.5 sm:w-2 sm:h-2 bg-msu-deep-ocean rounded-full animate-pulse"></div>
					<div class="absolute top-20 right-8 sm:top-40 sm:right-32 w-1 h-1 bg-msu-main-color rounded-full animate-pulse animation-delay-1000"></div>
					<div class="absolute bottom-16 left-1/4 sm:bottom-32 sm:left-1/3 w-2 h-2 sm:w-3 sm:h-3 bg-msu-deep-ocean rounded-full animate-pulse animation-delay-2000"></div>
					<div class="absolute bottom-10 right-5 sm:bottom-20 sm:right-20 w-1.5 h-1.5 sm:w-2 sm:h-2 bg-msu-main-color rounded-full animate-pulse animation-delay-3000"></div>
				</div>
			</div>

			<div class="container mx-auto px-4 sm:px-6 lg:px-8 relative z-20">
				<!-- Ultra-Premium Section Header - Mobile Optimized -->
				<div class="text-center mb-16 sm:mb-20 md:mb-24">
					<!-- Premium Badge - Responsive -->
					<div class="inline-flex items-center mb-6 sm:mb-8 relative">
						<div class="absolute inset-0 bg-gradient-to-r from-msu-deep-ocean/20 to-msu-main-color/20 dark:from-white/10 dark:to-gray-300/10 rounded-full blur-md sm:blur-lg"></div>
						<div class="relative bg-white/90 dark:bg-gray-800/90 backdrop-blur-xl border border-white/50 dark:border-gray-700/50 rounded-full px-4 py-2 sm:px-6 sm:py-3 md:px-8 md:py-4 shadow-xl">
							<div class="flex items-center space-x-2 sm:space-x-3 md:space-x-4">
								<div class="relative">
									<div class="w-2 h-2 sm:w-2.5 sm:h-2.5 md:w-3 md:h-3 bg-msu-deep-ocean dark:bg-white rounded-full animate-pulse"></div>
									<div class="absolute inset-0 w-2 h-2 sm:w-2.5 sm:h-2.5 md:w-3 md:h-3 bg-msu-deep-ocean dark:bg-white rounded-full animate-ping opacity-30"></div>
								</div>
								<span class="text-msu-deep-ocean dark:text-white font-bold tracking-wide sm:tracking-wider md:tracking-widest uppercase text-xs sm:text-sm">Distinguished Global Leaders</span>
								<div class="relative">
									<div class="w-2 h-2 sm:w-2.5 sm:h-2.5 md:w-3 md:h-3 bg-msu-main-color dark:bg-white rounded-full animate-pulse animation-delay-1000"></div>
									<div class="absolute inset-0 w-2 h-2 sm:w-2.5 sm:h-2.5 md:w-3 md:h-3 bg-msu-main-color dark:bg-white rounded-full animate-ping opacity-30 animation-delay-1000"></div>
								</div>
							</div>
						</div>
					</div>

					<!-- Massive Typography with Premium Animation - Fully Responsive -->
					<div class="relative mb-8 sm:mb-10 md:mb-12">
						<h2 class="text-4xl sm:text-5xl md:text-6xl lg:text-7xl xl:text-8xl 2xl:text-9xl font-black mb-2 sm:mb-3 md:mb-4 leading-tight sm:leading-none">
							<span class="block text-transparent bg-clip-text bg-gradient-to-r from-msu-deep-ocean via-msu-main-color to-msu-deep-ocean dark:from-white dark:via-gray-100 dark:to-white animate-gradient bg-300% transform hover:scale-105 transition-transform duration-700">
								KEYNOTE
							</span>
							<span class="block text-transparent bg-clip-text bg-gradient-to-r from-msu-main-color via-msu-deep-ocean to-msu-main-color dark:from-gray-100 dark:via-white dark:to-gray-100 animate-gradient bg-300% animation-delay-1000 transform hover:scale-105 transition-transform duration-700">
								SPEAKERS
							</span>
						</h2>
						
						<!-- Animated Underline - Responsive -->
						<div class="flex items-center justify-center mt-4 sm:mt-6 md:mt-8 mb-4 sm:mb-6 md:mb-8">
							<div class="w-16 sm:w-24 md:w-32 h-px bg-gradient-to-r from-transparent via-msu-deep-ocean to-transparent dark:via-white animate-pulse"></div>
							<div class="mx-3 sm:mx-4 md:mx-6 relative">
								<div class="w-3 h-3 sm:w-3.5 sm:h-3.5 md:w-4 md:h-4 bg-msu-deep-ocean dark:bg-white rounded-full animate-spin-slow"></div>
								<div class="absolute inset-0 w-3 h-3 sm:w-3.5 sm:h-3.5 md:w-4 md:h-4 bg-msu-main-color dark:bg-gray-300 rounded-full animate-ping opacity-20"></div>
							</div>
							<div class="w-16 sm:w-24 md:w-32 h-px bg-gradient-to-r from-transparent via-msu-deep-ocean to-transparent dark:via-white animate-pulse animation-delay-1000"></div>
						</div>
					</div>
					
					<!-- Enhanced Description - Mobile Optimized -->
					<div class="max-w-xl sm:max-w-3xl md:max-w-4xl lg:max-w-5xl mx-auto px-2 sm:px-0">
						<p class="text-lg sm:text-xl md:text-2xl lg:text-3xl text-gray-600 dark:text-gray-300 leading-relaxed font-light mb-3 sm:mb-4 md:mb-6">
							Experience transformative insights from the world's most
						</p>
						<p class="text-lg sm:text-xl md:text-2xl lg:text-3xl font-bold text-transparent bg-clip-text bg-gradient-to-r from-msu-deep-ocean to-msu-main-color dark:from-white dark:to-gray-300">
							visionary educators and technology pioneers
						</p>
					</div>
				</div>

				<!-- World-Class Main Speakers Grid - Mobile First Responsive -->
				<div class="grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-6 sm:gap-8 md:gap-10 lg:gap-12 mb-16 sm:mb-20 md:mb-24 lg:mb-32">
					<!-- Speaker 1 - Ultra Premium Design - Mobile Responsive -->
					<div class="group relative">
						<!-- Animated Background Glow -->
						<div class="absolute -inset-2 sm:-inset-3 md:-inset-4 bg-gradient-to-r from-msu-deep-ocean/30 via-msu-main-color/30 to-msu-deep-ocean/30 rounded-xl sm:rounded-2xl md:rounded-[2rem] blur-xl sm:blur-2xl group-hover:blur-2xl sm:group-hover:blur-3xl opacity-0 group-hover:opacity-100 transition-all duration-700"></div>
						
						<!-- Main Card -->
						<div class="relative bg-white/95 dark:bg-gray-800/95 backdrop-blur-2xl rounded-xl sm:rounded-2xl md:rounded-[2rem] p-6 sm:p-8 md:p-10 border border-white/60 dark:border-gray-700/60 shadow-xl sm:shadow-2xl hover:shadow-2xl sm:hover:shadow-4xl transition-all duration-700 group-hover:transform group-hover:-translate-y-2 sm:group-hover:-translate-y-4 group-hover:rotate-1">
							
							<!-- Speaker Image with Advanced Effects -->
							<div class="relative mb-6 sm:mb-8 md:mb-10">
								<div class="absolute -inset-1 sm:-inset-2 bg-gradient-to-r from-green-400 via-emerald-500 to-green-600 rounded-lg sm:rounded-xl md:rounded-[1.5rem] blur-md sm:blur-lg opacity-0 group-hover:opacity-60 transition-all duration-500"></div>
