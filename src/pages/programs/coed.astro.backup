---
import Layout from '../../layouts/Layout.astro';
import Navbar from "../../components/Navbar";
import FooterNav from "../../components/FooterNav.astro";
import Footer from "../../components/Footer.astro";
import '../../styles/css/global.css';
import '../../styles/css/globalanimate.css';


const currentPath = Astro.url.pathname;
---

<Layout title="College of Education | MSU-TCTO">
	<main class="font-sans">
		<Navbar path={currentPath} client:visible />

		<!-- Hero Section with Enhanced Video Background -->
		<section class="relative bg-msu-main-color h-screen min-h-[500px] md:min-h-[700px] flex items-center overflow-hidden pt-[80px] md:pt-[100px]">
			<!-- Enhanced Video Background with Parallax -->
			<div class="absolute inset-0 z-0 overflow-hidden">
				<video autoplay muted loop class="w-full h-full object-cover transform scale-110" id="heroVideo">
					<source src="/video/msucamp.mp4" type="video/mp4">
					<!-- Fallback image -->
					<!-- Professional CAS Icon -->
					<div class="w-full h-full flex items-center justify-center bg-gradient-to-r from-msu-main-color to-msu-deep-ocean p-8">
						<svg class="w-40 h-40 text-msu-bgc-color" viewBox="0 0 120 120" fill="none" xmlns="http://www.w3.org/2000/svg">
							<circle cx="60" cy="60" r="55" fill="none" stroke="currentColor" stroke-width="2" opacity="0.3"/>
							<circle cx="60" cy="60" r="45" fill="none" stroke="currentColor" stroke-width="1.5" opacity="0.5"/>
							<rect x="35" y="40" width="50" height="35" fill="none" stroke="currentColor" stroke-width="2"/>
							<rect x="40" y="45" width="40" height="25" fill="url(#buildingGradient)" stroke="currentColor" stroke-width="1"/>
							<rect x="45" y="50" width="8" height="8" fill="currentColor" opacity="0.7"/>
							<rect x="67" y="50" width="8" height="8" fill="currentColor" opacity="0.7"/>
							<rect x="45" y="62" width="8" height="8" fill="currentColor" opacity="0.7"/>
							<rect x="67" y="62" width="8" height="8" fill="currentColor" opacity="0.7"/>
							<rect x="55" y="65" width="10" height="10" fill="currentColor" opacity="0.8"/>
							<circle cx="25" cy="35" r="8" fill="url(#artsGradient)" stroke="currentColor" stroke-width="1.5"/>
							<circle cx="28" cy="32" r="2" fill="white"/>
							<circle cx="32" cy="36" r="2" fill="white"/>
							<circle cx="26" cy="40" r="2" fill="white"/>
							<circle cx="95" cy="35" r="8" fill="url(#sciencesGradient)" stroke="currentColor" stroke-width="1.5"/>
							<circle cx="95" cy="35" r="3" fill="currentColor"/>
							<ellipse cx="95" cy="35" rx="10" ry="4" fill="none" stroke="currentColor" stroke-width="1" transform="rotate(30 95 35)"/>
							<ellipse cx="95" cy="35" rx="10" ry="4" fill="none" stroke="currentColor" stroke-width="1" transform="rotate(-30 95 35)"/>
							<text x="25" y="85" text-anchor="middle" font-family="serif" font-size="14" font-weight="bold" fill="currentColor">âˆ‘</text>
							<path d="M85 80 Q90 75 95 80 Q100 85 105 80" fill="none" stroke="currentColor" stroke-width="2"/>
							<path d="M105 80 L110 75" stroke="currentColor" stroke-width="1.5"/>
							<circle cx="60" cy="20" r="6" fill="none" stroke="currentColor" stroke-width="1.5"/>
							<circle cx="60" cy="20" r="2" fill="currentColor"/>
							<path d="M60 20 L60 40" stroke="currentColor" stroke-width="1" opacity="0.5"/>
							<path d="M25 35 L35 40" stroke="currentColor" stroke-width="1" opacity="0.5"/>
							<path d="M95 35 L75 40" stroke="currentColor" stroke-width="1" opacity="0.5"/>
							<path d="M25 85 L35 75" stroke="currentColor" stroke-width="1" opacity="0.5"/>
							<path d="M105 80 L75 75" stroke="currentColor" stroke-width="1" opacity="0.5"/>
							<rect x="58" y="75" width="4" height="15" fill="currentColor" opacity="0.6"/>
							<defs>
								<linearGradient id="buildingGradient" x1="0%" y1="0%" x2="100%" y2="100%">
									<stop offset="0%" style="stop-color:var(--tw-gradient-from, #03045e);stop-opacity:0.2"/>
									<stop offset="100%" style="stop-color:var(--tw-gradient-to, #61063B);stop-opacity:0.3"/>
								</linearGradient>
								<linearGradient id="artsGradient" x1="0%" y1="0%" x2="100%" y2="100%">
									<stop offset="0%" style="stop-color:var(--tw-gradient-from, #03045e)"/>
									<stop offset="100%" style="stop-color:var(--tw-gradient-to, #61063B)"/>
								</linearGradient>
								<linearGradient id="sciencesGradient" x1="0%" y1="0%" x2="100%" y2="100%">
									<stop offset="0%" style="stop-color:var(--tw-gradient-from, #61063B)"/>
									<stop offset="100%" style="stop-color:var(--tw-gradient-to, #03045e)"/>
								</linearGradient>
							</defs>
						</svg>
						<div class="absolute bottom-6 left-1/2 transform -translate-x-1/2">
							<div class=" text-white px-4 py-2 rounded-full shadow-lg">
								<div class="text-center">
									<div class="text-sm font-bold">COLLEGE OF</div>
									<div class="text-lg font-bold">ARTS & SCIENCES</div>
								</div>
							</div>
						</div>
					</div>
				</video>
				<div class="absolute inset-0 bg-black/50"></div>
			</div>
			
			<!-- Enhanced Gradient overlay with animated particles -->
			<div class="absolute inset-0 bg-gradient-to-t from-msu-deep-ocean/95 via-msu-main-color/60 to-transparent z-1"></div>
			
			<!-- Animated floating particles -->
			<div class="absolute inset-0 z-2 overflow-hidden">
				<div class="particles-container">
					<div class="particle"></div>
					<div class="particle"></div>
					<div class="particle"></div>
					<div class="particle"></div>
					<div class="particle"></div>
					<div class="particle"></div>
				</div>
			</div>
			
			<div class="container mx-auto px-6 z-10 relative">
				<div class="max-w-5xl mx-auto text-center">
					<!-- Enhanced Logo with glow effect -->
					<div class="mb-4 animate-fadeInUp">
						<div class="relative inline-block">
							<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" class="h-32 w-32 mx-auto mb-3 text-white">
								<path fill="currentColor" d="M12 3L1 9l11 6 9-4.91V17h2V9M5 13.18v4L12 21l7-3.82v-4L12 17l-7-3.82Z"/>
							</svg>
							<div class="absolute inset-0 bg-msu-deep-ocean/20 blur-xl rounded-full animate-ping"></div>
						</div>
					</div>
					<!-- Enhanced Typography with gradient text using Poppins font -->
					<h1 class="text-3xl sm:text-4xl md:text-6xl lg:text-7xl xl:text-8xl font-bold mb-6 md:mb-8 leading-tight animate-fadeInUp px-4" style="font-family: 'Poppins', Arial, sans-serif; font-weight: 400;">
						<span class="bg-gradient-to-r from-msu-bgc-color via-msu-deep-ocean via-msu-deep-ocean to-msu-bgc-color bg-clip-text text-transparent">
							College of Education
						</span>
					</h1>
					<!-- Enhanced subtitle with animated underline -->
					<div class="relative mb-8 md:mb-12 animate-fadeInUp delay-100 px-4">
						<p class="text-lg sm:text-xl md:text-2xl lg:text-3xl text-white font-light max-w-3xl mx-auto leading-relaxed">
							Empowering educators, shaping the future.
						</p>
						<div class="absolute -bottom-6 md:-bottom-8 left-1/2 transform -translate-x-1/2 w-24 md:w-32 h-1 bg-gradient-to-r from-msu-bgc-color via-msu-deep-ocean via-msu-deep-ocean to-msu-bgc-color animate-pulse"></div>
					</div>
				
					<!-- Enhanced CTA buttons with hover effects -->
					<div class="flex flex-col sm:flex-row flex-wrap justify-center gap-4 sm:gap-6 animate-fadeInUp delay-200 mb-12 md:mb-16 lg:mb-20 px-4">
						<a 
							href="#programs" 
							class="group bg-gradient-to-r from-msu-main-color to-msu-deep-ocean hover:from-msu-deep-ocean hover:to-msu-main-color text-white font-bold py-3 px-6 sm:py-4 sm:px-10 rounded-full transition-all duration-500 inline-flex items-center justify-center hover:shadow-2xl hover:-translate-y-2 transform hover:scale-105 text-sm sm:text-base"
						>
							<span class="relative z-10">Explore Programs</span>
							<svg class="w-5 h-5 sm:w-6 sm:h-6 ml-2 sm:ml-3 group-hover:translate-x-1 transition-transform duration-300" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3"></path>
							</svg>
							<div class="absolute inset-0 bg-white/20 rounded-full blur opacity-0 group-hover:opacity-100 transition-opacity duration-300"></div>
						</a>
						<a 
							href="#contact" 
							class="group bg-transparent border-2 border-white hover:border-msu-deep-ocean text-white font-bold py-3 px-6 sm:py-4 sm:px-10 rounded-full transition-all duration-500 inline-flex items-center justify-center hover:shadow-2xl hover:-translate-y-2 transform hover:scale-105 backdrop-blur-sm text-sm sm:text-base"
						>
							<span class="relative z-10">Contact Us</span>
							<svg class="w-5 h-5 sm:w-6 sm:h-6 ml-2 sm:ml-3 group-hover:translate-x-1 transition-transform duration-300" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 10h.01M12 10h.01M16 10h.01M9 16H5a2 2 0 01-2-2V6a2 2 0 012-2h14a2 2 0 012 2v8a2 2 0 01-2 2h-5l-5 5v-5z"></path>
							</svg>
						</a>
					</div>
				</div>
			</div>
			
			<!-- Enhanced scrolling indicator with mobile responsiveness -->
			<div class="absolute bottom-2 md:bottom-4 left-1/2 transform -translate-x-1/2 z-10">
				<div class="flex flex-col items-center space-y-1 md:space-y-2">
					<span class="text-white text-xs md:text-sm font-light animate-pulse hidden sm:block">Scroll to explore</span>
					<div class="w-4 h-6 sm:w-5 sm:h-8 md:w-6 md:h-10 border-2 border-white rounded-full flex justify-center relative">
						<div class="w-1 h-1.5 sm:h-2 md:h-3 bg-white rounded-full mt-1 sm:mt-1.5 md:mt-2 animate-bounce"></div>
						<!-- Enhanced scroll indicator with dots -->
						<div class="absolute -bottom-3 sm:-bottom-4 md:-bottom-6 left-1/2 transform -translate-x-1/2 flex space-x-1">
							<div class="w-1 h-1 bg-white rounded-full animate-pulse"></div>
							<div class="w-1 h-1 bg-white rounded-full animate-pulse" style="animation-delay: 0.2s;"></div>
							<div class="w-1 h-1 bg-white rounded-full animate-pulse" style="animation-delay: 0.4s;"></div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<!-- University Seal & Accreditation -->
		<section class="py-8 md:py-12 bg-white">
			<div class="container mx-auto px-4 sm:px-6">
				<div class="flex flex-col md:flex-row items-center justify-center gap-6 md:gap-8 lg:gap-16">
					<div class="text-center">
						<img src="/images/MSU Seal New (Official).png" alt="MSU Seal" class="h-24 sm:h-28 md:h-32 mx-auto mb-3 md:mb-4 lazy-image" loading="lazy">
						<h3 class="text-base sm:text-lg font-semibold text-msu-bgc-color">Mindanao State University</h3>
						<p class="text-sm sm:text-base text-msu-main-color dark:text-msu-bgc-color">Tawi-Tawi College of Technology and Oceanography</p>
					</div>
					<div class="h-16 md:h-20 w-px bg-gray-200 hidden md:block"></div>
					<div class="text-center">
						<img src="/images/social media/aaccup.png" alt="AACCUP Accredited" class="h-24 sm:h-28 md:h-32 mx-auto mb-3 md:mb-4 lazy-image" loading="lazy">
						<h3 class="text-base sm:text-lg font-semibold text-msu-bgc-color">AACCUP Accredited</h3>
						<p class="text-sm sm:text-base text-msu-main-color dark:text-msu-bgc-color">Level III Re-Accredited Status</p>
					</div>
					<div class="h-16 md:h-20 w-px bg-gray-200 hidden md:block"></div>
					<div class="text-center">
						<img src="/images/social media/chedlogo.png" alt="CHED" class="h-24 sm:h-28 md:h-32 mx-auto mb-3 md:mb-4 lazy-image" loading="lazy">
						<h3 class="text-base sm:text-lg font-semibold text-msu-bgc-color">CHED</h3>
						<p class="text-sm sm:text-base text-msu-main-color dark:text-msu-bgc-color">Commission on Higher Education</p>
					</div>

					<div class="h-16 md:h-20 w-px bg-gray-200 hidden md:block"></div>
					<div class="text-center">
						<img src="/images/social media/ISO.png" alt="ISO 9001:2015 Certified" class="h-24 sm:h-28 md:h-32 mx-auto mb-3 md:mb-4 lazy-image" loading="lazy">
						<h3 class="text-base sm:text-lg font-semibold text-msu-bgc-color">ISO 9001:2025 Certified</h3>
						<p class="text-sm sm:text-base text-msu-main-color dark:text-msu-bgc-color">Quality Management System</p>
				</div>
			</div>
		</section>

		<!-- Enhanced About Section with Tabs -->
		<section class="py-12 md:py-16 lg:py-24 bg-gradient-to-br from-gray-50 to-white">
			<div class="container mx-auto px-4 sm:px-6">
				<div class="text-center mb-8 md:mb-12 lg:mb-16">
					<div class="flex justify-center mb-6">
						<div class="bg-gradient-to-br from-msu-deep-ocean to-msu-bgc-color p-4 rounded-full shadow-lg">
							<svg class="w-12 h-12 text-msu-main-color dark:text-msu-bgc-color" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6.253v13m0-13C10.832 5.477 9.246 5 7.5 5S4.168 5.477 3 6.253v13C4.168 18.477 5.754 18 7.5 18s3.332.477 4.5 1.253m0-13C13.168 5.477 14.754 5 16.5 5c1.746 0 3.332.477 4.5 1.253v13C19.832 18.477 18.246 18 16.5 18c-1.746 0-3.332.477-4.5 1.253"></path>
							</svg>
						</div>
					</div>
					<h2 class="text-3xl sm:text-4xl md:text-5xl font-bold text-msu-main-color dark:text-msu-bgc-color mb-4 md:mb-6">About the College of Education</h2>
					<p class="text-lg sm:text-xl text-msu-main-color dark:text-msu-bgc-color max-w-3xl mx-auto px-4">
						The College of Education, established in 1990, is a premier academic unit committed to developing highly skilled, values-driven, and globally competitive educators. Over the years, the College has continuously evolved to meet the challenges of the 21st-century learning environment.
					</p>
				</div>
				
				<div class="flex flex-col lg:flex-row items-start gap-8 md:gap-12 lg:gap-16">
					<div class="lg:w-1/2">
						<div class="relative rounded-2xl overflow-hidden shadow-2xl group cursor-pointer h-64 sm:h-80 md:h-96">
							<img src="/images/coed/coed.jpg" alt="College of Education Building" 
								class="w-full h-full object-cover transition-transform duration-700 group-hover:scale-110 lazy-image" loading="lazy" />
							
							<div class="absolute inset-0 bg-gradient-to-t from-msu-deep-ocean/80 via-transparent to-transparent flex items-end p-4 sm:p-6 md:p-8">
								<div class="transform translate-y-4 group-hover:translate-y-0 transition-transform duration-500">
									<h3 class="text-xl sm:text-2xl md:text-3xl font-bold text-white mb-2 md:mb-3">Excellence in Teacher Education</h3>
									<p class="text-gray-200 text-sm sm:text-base md:text-lg">Developing highly skilled, values-driven, and globally competitive educators</p>
								</div>
							</div>
							
							<!-- Floating elements -->
							<div class="absolute top-4 right-4 sm:top-6 sm:right-6 bg-msu-deep-ocean/90 backdrop-blur-sm text-white px-3 py-1.5 sm:px-4 sm:py-2 rounded-full text-xs sm:text-sm font-semibold">
								Est. 1990
							</div>
						</div>
					</div>
					
					<div class="lg:w-1/2">
						<div class="mb-6 md:mb-8 border-b border-gray-200">
							<nav class="-mb-px flex flex-col sm:flex-row space-y-2 sm:space-y-0 sm:space-x-4 md:space-x-8">
								<button class="tab-button active py-3 sm:py-4 px-1 border-b-2 font-medium text-xs sm:text-sm border-msu-deep-ocean text-msu-main-color dark:text-msu-bgc-color transition-all duration-300" data-tab="mission">
									<span class="flex items-center justify-center sm:justify-start">
										<svg class="w-4 h-4 sm:w-5 sm:h-5 mr-1 sm:mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
										</svg>
										<span class="hidden sm:inline">Mission</span>
										<span class="sm:hidden">Mission</span>
									</span>
								</button>
								<button class="tab-button py-3 sm:py-4 px-1 border-b-2 font-medium text-xs sm:text-sm border-transparent text-msu-main-color dark:text-msu-bgc-color hover:text-msu-deep-ocean hover:border-gray-300 transition-all duration-300" data-tab="vision">
									<span class="flex items-center justify-center sm:justify-start">
										<svg class="w-4 h-4 sm:w-5 sm:h-5 mr-1 sm:mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z"></path>
										</svg>
										<span class="hidden sm:inline">Vision</span>
										<span class="sm:hidden">Vision</span>
									</span>
								</button>
								<button class="tab-button py-3 sm:py-4 px-1 border-b-2 font-medium text-xs sm:text-sm border-transparent text-msu-main-color dark:text-msu-bgc-color hover:text-msu-deep-ocean hover:border-gray-300 transition-all duration-300" data-tab="history">
									<span class="flex items-center justify-center sm:justify-start">
										<svg class="w-4 h-4 sm:w-5 sm:h-5 mr-1 sm:mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
										</svg>
										<span class="hidden sm:inline">History</span>
										<span class="sm:hidden">History</span>
									</span>
								</button>
							</nav>
						</div>
						
						<div class="tab-content active" id="mission-content">
							<div class="space-y-6 text-msu-main-color dark:text-msu-bgc-color">
								<p class="text-xl leading-relaxed">
									The College of Education is mandated to provide quality teacher education through instruction, research, extension, and professional training. Its primary responsibility is to prepare future educators who are competent in curriculum development, instruction, and assessment; ethically grounded and committed to upholding professional standards; innovative and research-oriented, contributing to educational improvement; responsive to the needs of learners and society; and community-engaged, supporting lifelong learning and inclusive education.
								</p>
								<div class="grid grid-cols-1 md:grid-cols-2 gap-4">
									<div class="bg-gradient-to-r from-msu-deep-ocean/10 to-transparent p-4 rounded-lg border-l-4 border-msu-deep-ocean">
										<h4 class="font-semibold text-msu-main-color mb-2">Teacher Preparation</h4>
										<p class="text-sm">Comprehensive undergraduate and graduate programs that equip future educators with pedagogical expertise</p>
									</div>
									<div class="bg-gradient-to-r from-msu-deep-ocean/10 to-transparent p-4 rounded-lg border-l-4 border-msu-deep-ocean">
										<h4 class="font-semibold text-msu-main-color mb-2">Research Excellence</h4>
										<p class="text-sm">Active participation in research projects focused on pedagogy, assessment, and educational technology</p>
									</div>
									<div class="bg-gradient-to-r from-msu-deep-ocean/10 to-transparent p-4 rounded-lg border-l-4 border-msu-deep-ocean">
										<h4 class="font-semibold text-msu-main-color mb-2">Community Engagement</h4>
										<p class="text-sm">Extension programs addressing literacy improvement and teacher capacity-building in public schools</p>
									</div>
									<div class="bg-gradient-to-r from-msu-deep-ocean/10 to-transparent p-4 rounded-lg border-l-4 border-msu-deep-ocean">
										<h4 class="font-semibold text-msu-main-color mb-2">Professional Development</h4>
										<p class="text-sm">Seminars, workshops, and training programs for teachers and education stakeholders</p>
									</div>
								</div>
							</div>
						</div>
						
						<div class="tab-content hidden" id="vision-content">
							<div class="space-y-6 text-msu-main-color dark:text-msu-bgc-color">
								<p class="text-xl leading-relaxed">
									To be a premier center of teacher education in the Philippines, recognized for academic excellence, research, and leadership in developing highly skilled, values-driven, and globally competitive educators who contribute to the transformation of the Philippine educational landscape.
								</p>
								<div class="bg-gradient-to-r from-msu-deep-ocean/5 to-msu-deep-ocean/5 p-6 rounded-xl border border-msu-deep-ocean/20">
									<h4 class="font-bold text-msu-deep-ocean dark:text-msu-bgc-color mb-3">Our Aspirations</h4>
									<ul class="space-y-2 text-msu-main-color dark:text-msu-bgc-color">
										<li class="flex items-center">
											<svg class="w-4 h-4 text-msu-deep-ocean dark:text-msu-bgc-color mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
											</svg>
											Excellence in teacher education and pedagogical innovation
										</li>
										<li class="flex items-center">
											<svg class="w-4 h-4 text-msu-deep-ocean dark:text-msu-bgc-color mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
												<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
											</svg>
											Leadership in educational research and community engagement
										</li>
