async function openPDFViewer(pdfUrl, title) {
	console.log('Opening professional PDF viewer:', { pdfUrl, title });
	
	// Check if PDF URL is valid
	if (!pdfUrl || pdfUrl === '' || pdfUrl === '#') {
		console.error('Invalid PDF URL:', pdfUrl);
