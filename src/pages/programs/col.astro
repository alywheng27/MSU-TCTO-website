	<script>
		document.addEventListener('DOMContentLoaded', function() {
			// Student Life Slider Functionality
			const sliderTrack = document.getElementById('slider-track');
			const prevBtn = document.getElementById('prevBtn');
			const nextBtn = document.getElementById('nextBtn');
			const dots = document.querySelectorAll('.slider-dot');

			let currentSlide = 0;
			let isAutoplay = true;
			let autoplayInterval;
			const totalSlides = 6;

			// Get slide width based on screen size
			function getSlideWidth() {
				const screenWidth = window.innerWidth;
				if (screenWidth < 768) return 100; // Mobile: 1 slide
				if (screenWidth < 1024) return 50; // Tablet: 2 slides
				return 25; // Desktop: 4 slides
			}

			// Update slider position with smooth transitions
			function updateSlider() {
				const slideWidth = getSlideWidth();
				const translateX = -(currentSlide * slideWidth);
				
				// Add transitioning class for smooth effects
				sliderTrack.classList.add('transitioning');
				
				// Use requestAnimationFrame for smoother animation
				requestAnimationFrame(() => {
					sliderTrack.style.transform = `translateX(${translateX}%)`;
					
					// Remove transitioning class after animation completes
					setTimeout(() => {
						sliderTrack.classList.remove('transitioning');
					}, 800); // Match CSS transition duration
				});
				
				// Update dots with smooth transition
				dots.forEach((dot, index) => {
					const isActive = index === currentSlide;
					dot.classList.toggle('active', isActive);
					
					// Add smooth scale effect to active dot
					if (isActive) {
						(dot as HTMLElement).style.transform = 'scale(1.2)';
					} else {
						(dot as HTMLElement).style.transform = 'scale(1)';
					}
				});
			}

			// Next slide
			function nextSlide() {
				currentSlide = (currentSlide + 1) % totalSlides;
				updateSlider();
			}

			// Previous slide
			function prevSlide() {
				currentSlide = (currentSlide - 1 + totalSlides) % totalSlides;
				updateSlider();
			}

			// Go to specific slide
			function goToSlide(slideIndex) {
				currentSlide = slideIndex;
				updateSlider();
			}

			// Start autoplay
			function startAutoplay() {
				if (autoplayInterval) clearInterval(autoplayInterval);
				autoplayInterval = setInterval(nextSlide, 3000); // 3 seconds for more dynamic feel
			}

			// Stop autoplay
			function stopAutoplay() {
				if (autoplayInterval) {
					clearInterval(autoplayInterval);
					autoplayInterval = null;
				}
			}


			// Event listeners
			nextBtn.addEventListener('click', () => {
				nextSlide();
				if (isAutoplay) {
					stopAutoplay();
					setTimeout(startAutoplay, 1000); // Restart after 1 second
				}
			});

			prevBtn.addEventListener('click', () => {
				prevSlide();
				if (isAutoplay) {
					stopAutoplay();
					setTimeout(startAutoplay, 1000); // Restart after 1 second
				}
			});


			// Dot navigation
			dots.forEach((dot, index) => {
				dot.addEventListener('click', () => {
					goToSlide(index);
					if (isAutoplay) {
						stopAutoplay();
						setTimeout(startAutoplay, 1000); // Restart after 1 second
					}
				});
			});

			// Pause autoplay on hover
			const sliderContainer = document.querySelector('.student-life-slider');
			sliderContainer.addEventListener('mouseenter', stopAutoplay);
			sliderContainer.addEventListener('mouseleave', () => {
				if (isAutoplay) startAutoplay();
			});

			// Touch/swipe support for mobile
			let startX = 0;
			let startY = 0;
			let distX = 0;
			let distY = 0;

			sliderContainer.addEventListener('touchstart', (e) => {
				const touchEvent = e as TouchEvent;
				const touch = touchEvent.touches[0];
				startX = touch.clientX;
				startY = touch.clientY;
			});

			sliderContainer.addEventListener('touchmove', (e) => {
				e.preventDefault(); // Prevent scrolling
			});

			sliderContainer.addEventListener('touchend', (e) => {
				const touchEvent = e as TouchEvent;
				const touch = touchEvent.changedTouches[0];
				distX = touch.clientX - startX;
				distY = touch.clientY - startY;

				// Only trigger if horizontal swipe is greater than vertical
				if (Math.abs(distX) > Math.abs(distY) && Math.abs(distX) > 50) {
					if (distX > 0) {
						prevSlide();
					} else {
						nextSlide();
					}
					if (isAutoplay) {
						stopAutoplay();
						setTimeout(startAutoplay, 1000);
					}
				}
			});

			// Keyboard navigation
			document.addEventListener('keydown', (e) => {
				if (e.key === 'ArrowLeft') {
					prevSlide();
					if (isAutoplay) {
						stopAutoplay();
						setTimeout(startAutoplay, 1000);
					}
				} else if (e.key === 'ArrowRight') {
					nextSlide();
					if (isAutoplay) {
						stopAutoplay();
						setTimeout(startAutoplay, 1000);
					}
				}
			});

			// Handle window resize
			window.addEventListener('resize', () => {
				updateSlider();
			});

			// Initialize slider
			updateSlider();
			
			// Start autoplay immediately with a small delay for smooth initialization
			setTimeout(() => {
				if (isAutoplay) startAutoplay();
			}, 1000);

			// Add entrance animations to slides with smooth preloading
			const slides = document.querySelectorAll('.slider-slide');
			slides.forEach((slide, index) => {
				(slide as HTMLElement).style.animationDelay = `${index * 0.1}s`;
				slide.classList.add('slide-enter-right');
				
				// Preload images for smoother transitions
				const img = slide.querySelector('img') as HTMLImageElement;
				if (img) {
					// Add loading optimization
					img.addEventListener('load', () => {
						img.style.opacity = '1';
						img.style.transition = 'opacity 0.3s ease';
					});
					
					// Set initial opacity for smooth loading
					img.style.opacity = '0';
				}
			});

			// Add smooth hover effects to navigation buttons
			const navButtons = document.querySelectorAll('.nav-button');
			navButtons.forEach(button => {
				button.addEventListener('mouseenter', () => {
					(button as HTMLElement).style.transform = 'scale(1.1)';
					(button as HTMLElement).style.transition = 'transform 0.2s ease';
				});
				
				button.addEventListener('mouseleave', () => {
					(button as HTMLElement).style.transform = 'scale(1)';
				});
			});
		});
	</script>

</Layout>
