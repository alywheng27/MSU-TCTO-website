	// Close PDF viewer with Escape key
	document.addEventListener('keydown', function(e) {
		if (e.key === 'Escape') {
			const modal = document.getElementById('pdfViewerModal');
			if (modal && !modal.classList.contains('hidden')) {
				closePdfViewer();
			}
		}
	});
