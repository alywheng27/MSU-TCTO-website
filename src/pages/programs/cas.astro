	function resetProgramsDisplay() {
		const banners = document.querySelectorAll('.faculty-banner-programs');
		banners.forEach((container) => {
			const element = container as HTMLElement;
			element.innerHTML = '';
			element.classList.add('hidden');
			element.style.display = 'none';
		});
	}

	function updateProgramsDisplay() {
		const banners = document.querySelectorAll('.faculty-department-banner');
		banners.forEach((banner) => {
			const department = banner.getAttribute('data-department') || '';
			const programsContainer = banner.querySelector('.faculty-banner-programs') as HTMLElement | null;
			if (!programsContainer) return;

			const defaultDisplay = programsContainer.dataset.defaultDisplay || (programsContainer.classList.contains('grid') ? 'grid' : 'block');
			if (!programsContainer.dataset.defaultDisplay) {
				programsContainer.dataset.defaultDisplay = defaultDisplay;
			}

			if (!activeProgramDepartment || department !== activeProgramDepartment) {
				programsContainer.innerHTML = '';
				programsContainer.classList.add('hidden');
				programsContainer.style.display = 'none';
				return;
			}

			renderProgramsForDepartment(department, programsContainer);
		});
	}

	function updateBannerContent(cards: HTMLElement[]) {
		const banners = document.querySelectorAll('.faculty-department-banner');
		banners.forEach((banner) => {
			const department = banner.getAttribute('data-department') || '';
			const contentContainer = banner.querySelector('.faculty-banner-content') as HTMLElement | null;
			if (!contentContainer) return;
			const defaultDisplay = contentContainer.dataset.defaultDisplay || (contentContainer.classList.contains('grid') ? 'grid' : 'block');
			if (!contentContainer.dataset.defaultDisplay) {
				contentContainer.dataset.defaultDisplay = defaultDisplay;
			}
			const isStaticContent = contentContainer.dataset.staticContent === 'true';
			if (isStaticContent) {
				if (!activeDepartmentFilter || department !== activeDepartmentFilter) {
					contentContainer.classList.add('hidden');
					contentContainer.style.display = 'none';
				} else {
					contentContainer.classList.remove('hidden');
					contentContainer.style.display = defaultDisplay;
				}
				return;
			}
			if (!activeDepartmentFilter || department !== activeDepartmentFilter) {
				contentContainer.innerHTML = '';
				contentContainer.classList.add('hidden');
				contentContainer.style.display = 'none';
				return;
			}
			const fragment = document.createDocumentFragment();
			cards.forEach((card) => {
				const name = card.querySelector('h3')?.textContent?.trim() || '';
				const designation = card.querySelector('p.text-gray-300')?.textContent?.trim() || '';
				const wrapper = document.createElement('div');
				wrapper.className = 'flex items-start gap-3 bg-white/10 rounded-xl px-4 py-3 backdrop-blur-sm';
				wrapper.innerHTML = `
					<svg class="w-5 h-5 text-yellow-300 mt-1" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
						<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
					</svg>
					<div class="leading-tight">
						<p class="font-semibold">${escapeHtml(name)}</p>
						${designation ? `<p class="text-white/80 text-sm font-normal">${escapeHtml(designation)}</p>` : ''}
					</div>
				`;
				fragment.appendChild(wrapper);
			});
			contentContainer.innerHTML = '';
			contentContainer.appendChild(fragment);
			contentContainer.classList.remove('hidden');
			contentContainer.style.display = defaultDisplay;
		});
	}

	function renderFacultyCardsInBanner(department: string, cards: HTMLElement[]) {
		// Hide all banner card containers first
		const allBannerContainers = document.querySelectorAll('.faculty-banner-cards-container');
		allBannerContainers.forEach((container) => {
			const containerElement = container as HTMLElement;
			containerElement.classList.add('hidden');
			const grid = containerElement.querySelector('.grid');
			if (grid) {
				grid.innerHTML = '';
			}
		});

		// Find the container for this department
		const bannerContainer = Array.from(allBannerContainers).find((container) => {
			return container.getAttribute('data-department') === department;
		}) as HTMLElement | null;

		if (!bannerContainer) return;

		const grid = bannerContainer.querySelector('.grid');
		if (!grid) return;

		// Clear existing content
		grid.innerHTML = '';

		// Filter cards to ensure they belong to this specific department and ensure uniqueness by faculty ID
		const seenFacultyIds = new Set<string>();
		const uniqueCards: HTMLElement[] = [];

		cards.forEach((card) => {
			const cardDepartment = (card as HTMLElement).dataset.department || '';
			const facultyId = (card as HTMLElement).dataset.facultyId || '';
			
			// Only include cards that match this exact department and haven't been seen before
			if (cardDepartment === department && facultyId && !seenFacultyIds.has(facultyId)) {
				seenFacultyIds.add(facultyId);
				uniqueCards.push(card);
			}
		});

		// Clone and append unique faculty cards
		uniqueCards.forEach((card) => {
			const clone = card.cloneNode(true) as HTMLElement;
			clone.classList.remove('hidden');
			clone.style.display = '';
			clone.removeAttribute('id');
			grid.appendChild(clone);
		});

		// Show the container
		bannerContainer.classList.remove('hidden');

		// Initialize lazy loading for images in the banner
		const bannerImages = bannerContainer.querySelectorAll<HTMLImageElement>('.lazy-image');
		if ('IntersectionObserver' in window) {
			const observer = new IntersectionObserver((entries, observerInstance) => {
				entries.forEach(entry => {
					if (entry.isIntersecting) {
						const img = entry.target as HTMLImageElement;
						img.classList.add('loaded');
						(img as HTMLElement).style.opacity = '1';
						observerInstance.unobserve(img);
					}
				});
			}, {
				rootMargin: '50px 0px',
				threshold: 0.01
			});

			bannerImages.forEach(img => {
				if (!img.classList.contains('loaded')) {
					(img as HTMLElement).style.opacity = '0';
					observer.observe(img);
				}
			});
		} else {
			bannerImages.forEach(img => {
				img.classList.add('loaded');
				(img as HTMLElement).style.opacity = '1';
			});
		}

		// Scroll to the banner container smoothly
		bannerContainer.scrollIntoView({ behavior: 'smooth', block: 'start' });
	}

	function applyDepartmentFilter() {
		const facultyGrid = document.getElementById('facultyGrid');
		
		if (!activeDepartmentFilter) {
			// Hide all faculty cards when no department is selected
			if (facultyGrid) {
				facultyGrid.classList.add('hidden');
			}
			allFacultyCards.forEach((card) => {
				card.classList.add('hidden');
				(card as HTMLElement).style.display = 'none';
			});
			if (showMoreBtn) {
				showMoreBtn.classList.add('hidden');
			}
			updateBannerContent([]);
			
			// Hide all banner card containers
			const allBannerContainers = document.querySelectorAll('.faculty-banner-cards-container');
			allBannerContainers.forEach((container) => {
				(container as HTMLElement).classList.add('hidden');
				const grid = container.querySelector('.grid');
				if (grid) {
					grid.innerHTML = '';
				}
			});
			return;
		}

		// Keep faculty grid hidden when showing cards in banners
		if (facultyGrid) {
			facultyGrid.classList.add('hidden');
		}

		let visibleCount = 0;
		const departmentCards: HTMLElement[] = [];
		const seenFacultyIds = new Set<string>();
		
		allFacultyCards.forEach((card) => {
			const department = (card as HTMLElement).dataset.department || '';
			const facultyId = (card as HTMLElement).dataset.facultyId || '';
			const matchesDepartment = department === activeDepartmentFilter;
			
			if (matchesDepartment) {
				// Ensure uniqueness by faculty ID - only add if we haven't seen this faculty member yet
				if (facultyId && !seenFacultyIds.has(facultyId)) {
					seenFacultyIds.add(facultyId);
					card.classList.add('hidden');
					(card as HTMLElement).style.display = 'none';
					visibleCount++;
					departmentCards.push(card as HTMLElement);
				} else {
					// Hide duplicate cards
					card.classList.add('hidden');
					(card as HTMLElement).style.display = 'none';
				}
			} else {
				card.classList.add('hidden');
				(card as HTMLElement).style.display = 'none';
			}
		});

		// Update banner content with visible faculty cards (summary)
		updateBannerContent(departmentCards);

		// Render full faculty cards in the banner container
		renderFacultyCardsInBanner(activeDepartmentFilter, departmentCards);

		// Hide "Show More" button when filtering by department
		if (showMoreBtn) {
			showMoreBtn.classList.add('hidden');
		}

		// Clear search when filtering by department
		if (facultySearchInput) {
			(facultySearchInput as HTMLInputElement).value = '';
		}
		if (facultySearchResults) {
			facultySearchResults.classList.add('hidden');
		}
		if (clearFacultySearchBtn) {
			clearFacultySearchBtn.classList.add('hidden');
		}
	}

	// Add event listeners to department buttons
	if (departmentFacultyButtons.length) {
		departmentFacultyButtons.forEach((button) => {
			button.addEventListener('click', (event) => {
				event.preventDefault();
				const department = button.dataset.department;
				if (!department) return;
				const isActive = activeDepartmentFilter === department;
				activeDepartmentFilter = isActive ? null : department;
				applyDepartmentFilter();
				updateDepartmentButtonsState();
				// Hide programs when showing faculty
				if (activeDepartmentFilter) {
					activeProgramDepartment = null;
					resetProgramsDisplay();
				}
			});
		});
	}

	// Add event listeners to program buttons - open modal instead
	if (departmentProgramButtons.length) {
		departmentProgramButtons.forEach((button) => {
			button.addEventListener('click', (event) => {
				event.preventDefault();
				const rawDepartment = button.dataset.department || '';
				const searchTerm = button.dataset.departmentSearch || rawDepartment || button.textContent || '';
				const department = searchTerm.trim();
				if (!department) return;
				openDepartmentProgramsModal(department);
			});
		});
	}

	updateDepartmentButtonsState();
});

// Alumni Carousel Functionality
let currentAlumniSlide = 0;
let alumniAutoPlay = true;
let alumniInterval;

function initAlumniCarousel() {
	const carousel = document.getElementById('alumniCarousel');
	const dots = document.querySelectorAll('.alumni-dot');
	const prevBtn = document.getElementById('prevAlumni');
	const nextBtn = document.getElementById('nextAlumni');
	
	if (!carousel || !dots.length || !prevBtn || !nextBtn) {
		console.error('Alumni carousel elements not found');
		return;
	}
	
	// Update carousel position
	function updateCarousel() {
		carousel.style.transform = `translateX(-${currentAlumniSlide * 100}%)`;
		
		// Update dots
		dots.forEach((dot, index) => {
			if (index === currentAlumniSlide) {
				dot.classList.remove('bg-white/50');
				dot.classList.add('bg-white');
			} else {
				dot.classList.remove('bg-white');
				dot.classList.add('bg-white/50');
			}
		});
	}
	
	// Next slide
	function nextSlide() {
		currentAlumniSlide = (currentAlumniSlide + 1) % 4;
		updateCarousel();
	}
	
	// Previous slide
	function prevSlide() {
		currentAlumniSlide = (currentAlumniSlide - 1 + 4) % 4;
		updateCarousel();
	}
	
	// Go to specific slide
	function goToSlide(slideIndex) {
		currentAlumniSlide = slideIndex;
		updateCarousel();
	}
	
	// Auto-play functionality
	function startAutoPlay() {
		if (alumniAutoPlay) {
			alumniInterval = setInterval(nextSlide, 5000); // Change slide every 5 seconds
		}
	}
	
	function stopAutoPlay() {
		clearInterval(alumniInterval);
	}
	
	
	// Event listeners
	nextBtn.addEventListener('click', () => {
		nextSlide();
		if (alumniAutoPlay) {
			stopAutoPlay();
			startAutoPlay();
		}
	});
	
	prevBtn.addEventListener('click', () => {
		prevSlide();
		if (alumniAutoPlay) {
			stopAutoPlay();
			startAutoPlay();
		}
	});
	
	dots.forEach((dot, index) => {
		dot.addEventListener('click', () => {
			goToSlide(index);
			if (alumniAutoPlay) {
				stopAutoPlay();
				startAutoPlay();
			}
		});
	});
	
	
	// Touch/swipe support
	let startX = 0;
	let endX = 0;
	
	carousel.addEventListener('touchstart', (e) => {
		startX = e.touches[0].clientX;
	});
	
	carousel.addEventListener('touchend', (e) => {
		endX = e.changedTouches[0].clientX;
		handleSwipe();
	});
	
	function handleSwipe() {
		const threshold = 50;
		const diff = startX - endX;
		
		if (Math.abs(diff) > threshold) {
			if (diff > 0) {
				nextSlide(); // Swipe left - next slide
			} else {
				prevSlide(); // Swipe right - previous slide
			}
			
			if (alumniAutoPlay) {
				stopAutoPlay();
				startAutoPlay();
			}
		}
	}
	
	// Keyboard navigation
	document.addEventListener('keydown', (e) => {
		if (e.key === 'ArrowLeft') {
			prevSlide();
			if (alumniAutoPlay) {
				stopAutoPlay();
				startAutoPlay();
			}
		} else if (e.key === 'ArrowRight') {
			nextSlide();
			if (alumniAutoPlay) {
				stopAutoPlay();
				startAutoPlay();
			}
		}
	});
	
	// Initialize
	updateCarousel();
	startAutoPlay();
	
	// Pause auto-play when page is not visible
	document.addEventListener('visibilitychange', () => {
		if (document.hidden) {
			stopAutoPlay();
		} else if (alumniAutoPlay) {
			startAutoPlay();
		}
	});
}

