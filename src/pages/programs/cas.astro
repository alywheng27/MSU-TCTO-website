	// Department Programs Modal Functions
	function normalizeDepartmentValue(value: string) {
		return (value || '')
			.toLowerCase()
			.replace(/&/g, 'and')
			.replace(/\s+/g, ' ')
			.trim();
	}

	const departmentDisplayLookup: Record<string, string> = {
		[normalizeDepartmentValue('Department of Literary and Cultural Studies')]: 'Department of Literary and Cultural Studies',
		[normalizeDepartmentValue('Department of Languages')]: 'Department of Languages',
		[normalizeDepartmentValue('Department of Mathematics')]: 'Department of Mathematics',
		[normalizeDepartmentValue('Department of Statistics')]: 'Department of Statistics',
		[normalizeDepartmentValue('Department of History')]: 'Department of History',
		[normalizeDepartmentValue('Department of Public Administration')]: 'Department of Public Administration',
		[normalizeDepartmentValue('Department of Political Science')]: 'Department of Political Science',
		[normalizeDepartmentValue('Department of Management')]: 'Department of Management',
		[normalizeDepartmentValue('College of Arts and Social Sciences (CASS)')]: 'College of Arts and Social Sciences (CASS)'
	};

	function resolveDepartmentPrograms(department: string) {
		const rawSearchLower = (department || '').toLowerCase();
		const normalizedInput = normalizeDepartmentValue(department);

		const programs = bannerProgramsData.filter((program: any) => {
			if (!program) return false;
			const candidates = [
				program.department,
				program.department?.department,
				program.program,
				program.name,
				program.title
			];
			return candidates.some((value) => {
				if (!value) return false;
				const text = typeof value === 'string'
					? value
					: (value && typeof value === 'object' && 'toString' in value)
						? String(value)
						: '';
				const normalizedValue = normalizeDepartmentValue(text);
				return normalizedValue === normalizedInput ||
					normalizedValue.includes(normalizedInput) ||
					normalizedInput.includes(normalizedValue) ||
					text.toLowerCase().includes(rawSearchLower);
			});
		});

		return {
			label: department,
			programs: programs
		};
	}

