	// Function to handle faculty members assigned to multiple departments
	function processMultiDepartmentFaculty(facultyId: string, facultyData: any) {
		// Check if faculty member has multiple departments assigned
		if (facultyData.departments && Array.isArray(facultyData.departments) && facultyData.departments.length > 1) {
			// Create a card for each department (except the primary one which is already handled)
			facultyData.departments.forEach((dept: string, index: number) => {
				// Skip the first department if it matches the primary department
				if (index === 0 && dept === facultyData.department) {
					return; // Primary department card already exists
				}
				
				// Create a unique key for this department assignment
				const uniqueKey = `${facultyId}-${dept.replace(/\s+/g, '-').toLowerCase()}`;
				
				// Check if we already have a card for this faculty-department combination
				if (!facultyCardMap.has(uniqueKey)) {
					// Note: In a real implementation, you would create/clone the card here
					// For now, we're just tracking it in the map
					console.log(`Faculty ${facultyId} assigned to multiple departments: ${dept}`);
				}
			});
		}
	}

