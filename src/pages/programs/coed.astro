import Layout from '../../layouts/Layout.astro';

import Navbar from "../../components/Navbar";

import FooterNav from "../../components/FooterNav.astro";

import Footer from "../../components/Footer.astro";

import PDFViewer from "../../components/PDFViewer.jsx";

import '../../styles/css/global.css';

import '../../styles/css/globalanimate.css';

import '../../styles/css/pdf-viewer.css';

import { getCOEDPrograms } from '../../api/api.jsx';





const currentPath = Astro.url.pathname;

// Fetch COED programs data from Sanity
type ProgramDataItem = {
  _id: string;
  program: string;
  description: string;
  degree: string;
  level: string;
  duration: string;
  accreditation: string;
  fileUrl: string | null;
  name: string;
  department: string;
};

let programsData: ProgramDataItem[] = [];
let filterOptions = {
  degrees: [] as string[],
  levels: [] as string[],
  departments: [] as string[]
};

// Helper function to extract text from Sanity block content
function extractTextFromBlocks(blocks: any[]): string {
  if (!blocks || !Array.isArray(blocks)) return '';
  
  return blocks
    .map(block => {
      if (block._type === 'block' && block.children) {
        return block.children
          .map((child: any) => child.text || '')
          .join('');
      }
      return '';
    })
    .join(' ')
    .trim();
}

try {
  const rawProgramsData = await getCOEDPrograms();
  
  // Transform Sanity data to match frontend expectations
  programsData = rawProgramsData.map(program => ({
    _id: program._id,
    program: program.title,
    description: extractTextFromBlocks(program.description) || '',
    degree: program.degree?.degree || '',
    level: program.level?.level || '',
    duration: program.duration?.duration || '',
    accreditation: program.accreditation || '',
    fileUrl: program.file?.asset?.url || null,
    name: program.title,
    department: program.department?.department || ''
  }));
  
  // Extract unique filter options
  const degreeSet = new Set<string>();
  const levelSet = new Set<string>();
  const departmentSet = new Set<string>();
  
  programsData.forEach(program => {
    if (program.degree) degreeSet.add(program.degree);
    if (program.level) levelSet.add(program.level);
    if (program.department) departmentSet.add(program.department);
  });
  
  // Convert Sets to Arrays for easier use
  filterOptions.degrees = Array.from(degreeSet);
  filterOptions.levels = Array.from(levelSet);
  filterOptions.departments = Array.from(departmentSet);
  
  console.log('COED Programs loaded:', programsData.length);
  console.log('Filter options:', filterOptions);
  console.log('Sample program:', programsData[0]);
  console.log('Sample program description:', programsData[0]?.description);
  console.log('Raw description data:', rawProgramsData[0]?.description);
} catch (error) {
  console.error('Error fetching COED programs data:', error);
  // Fallback to empty array if fetch fails
  programsData = [];
}

const targetDepartments = [
  'Department of Early Childhood and Elementary Education',
  'Department of Language Education',
  'Department of Science and Mathematics Education',
  'Department of Physical Education',
  'Department of Educational Leadership and Professional Services'
];

const normalizeDepartmentName = (value: string) =>
  (value || '')
    .toLowerCase()
    .replace(/&/g, 'and')
    .replace(/\s+/g, ' ')
    .trim();

const departmentProgramsMap = targetDepartments.reduce<Record<string, ProgramDataItem[]>>((acc, department) => {
  const normalizedTarget = normalizeDepartmentName(department);
  acc[department] = programsData.filter((program) => {
    const normalizedProgramDepartment = normalizeDepartmentName(program.department);
    return normalizedProgramDepartment === normalizedTarget ||
      normalizedProgramDepartment.includes(normalizedTarget) ||
      normalizedTarget.includes(normalizedProgramDepartment);
  });
  return acc;
}, {});

console.log('Department programs map created for departments:', Object.keys(departmentProgramsMap));

