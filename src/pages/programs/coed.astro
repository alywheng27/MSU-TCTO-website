	}

	function programsMatchDepartment(programDepartment: string, targetDepartment: string) {
		if (!programDepartment || !targetDepartment) return false;
		const normalizedProgram = normalizeDepartmentName(programDepartment);
		const normalizedTarget = normalizeDepartmentName(targetDepartment);
		return normalizedProgram === normalizedTarget ||
			normalizedProgram.includes(normalizedTarget) ||
			normalizedTarget.includes(normalizedProgram);
