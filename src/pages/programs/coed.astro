	}

	function updateProgramsDisplay() {
		const banners = document.querySelectorAll('.faculty-department-banner');
		banners.forEach((banner) => {
			const department = banner.getAttribute('data-department') || '';
			const programsContainer = banner.querySelector('.faculty-banner-programs') as HTMLElement | null;
			if (!programsContainer) return;

			const defaultDisplay = programsContainer.dataset.defaultDisplay || (programsContainer.classList.contains('grid') ? 'grid' : 'block');
			if (!programsContainer.dataset.defaultDisplay) {
				programsContainer.dataset.defaultDisplay = defaultDisplay;
			}

			// Only show programs for the exact active department (clicked department)
			// Always hide if not active to prevent auto-opening
			if (!activeDepartmentFilter || department !== activeDepartmentFilter) {
				programsContainer.innerHTML = '';
				programsContainer.classList.add('hidden');
				programsContainer.style.display = 'none';
				return;
			}

			// Only render if this is the exact active department
			if (department === activeDepartmentFilter) {
				renderProgramsForDepartment(department, programsContainer);
			}
		});
	}

	function applyDepartmentFilter() {
		const departmentCards = [] as HTMLElement[];
		if (!activeDepartmentFilter) {
			hideDepartmentNotice();
			facultyCardMap.forEach((card) => {
				const element = card as HTMLElement;
				element.classList.remove('hidden');
				element.style.display = '';
			});
			hideAdditionalFacultyCards();
			updateBannerContent([]);
			resetProgramsDisplay();
			// Explicitly hide all banners when no department is selected
			const allBanners = document.querySelectorAll('.faculty-department-banner');
			allBanners.forEach((banner) => {
				const bannerContent = banner.querySelector('.faculty-banner-content') as HTMLElement | null;
				const programsContainer = banner.querySelector('.faculty-banner-programs') as HTMLElement | null;
				if (bannerContent) {
					bannerContent.classList.add('hidden');
					bannerContent.style.display = 'none';
					bannerContent.innerHTML = '';
				}
				if (programsContainer) {
					programsContainer.classList.add('hidden');
					programsContainer.style.display = 'none';
					programsContainer.innerHTML = '';
				}
			});
			return;
		}

		clearActiveHighlight();
		
		// First, hide all banners except the one being clicked
		// This ensures no other banners open when switching departments
		const allBanners = document.querySelectorAll('.faculty-department-banner');
		allBanners.forEach((banner) => {
			const department = banner.getAttribute('data-department') || '';
			const bannerContent = banner.querySelector('.faculty-banner-content') as HTMLElement | null;
			const programsContainer = banner.querySelector('.faculty-banner-programs') as HTMLElement | null;
			// Only keep open the banner for the active department
			if (department !== activeDepartmentFilter) {
				if (bannerContent) {
					bannerContent.classList.add('hidden');
					bannerContent.style.display = 'none';
					bannerContent.innerHTML = '';
				}
				if (programsContainer) {
					programsContainer.classList.add('hidden');
					programsContainer.style.display = 'none';
					programsContainer.innerHTML = '';
				}
			}
		});

		let visibleCount = 0;
		const seenFacultyIdsInDepartment = new Set<string>();
		
		// Helper function to check if faculty member belongs to a department (including multi-department assignments)
		function facultyBelongsToDepartment(facultyId: string, targetDepartment: string): boolean {
			const facultyData = facultySearchData[facultyId];
			if (!facultyData) return false;
			
			// Check if faculty has multiple departments assigned
			if (facultyData.departments && Array.isArray(facultyData.departments)) {
				return facultyData.departments.includes(targetDepartment);
			}
			// Fallback to single department
			return facultyData.department === targetDepartment;
		}
		
		// Process cards from map
		facultyCardMap.forEach((card) => {
			const cardElement = card as HTMLElement;
			const department = cardElement.dataset.department || '';
			const facultyId = cardElement.dataset.facultyId || '';
			const baseFacultyId = facultyId.replace(/-[^-]+$/, ''); // Remove department suffix if exists
			
			// Check if this faculty member belongs to the active department
			// This handles both single department and multi-department assignments
			const matchesDepartment = department === activeDepartmentFilter || 
				facultyBelongsToDepartment(baseFacultyId, activeDepartmentFilter);
			
			if (matchesDepartment) {
				// Ensure uniqueness by faculty ID within this department - use the card's unique ID
				// This allows the same faculty member to appear in multiple departments if they have separate cards
				const uniqueKey = `${facultyId}-${activeDepartmentFilter}`;
				if (facultyId && !seenFacultyIdsInDepartment.has(uniqueKey)) {
					seenFacultyIdsInDepartment.add(uniqueKey);
					cardElement.classList.remove('hidden');
					cardElement.style.display = '';
					visibleCount += 1;
					departmentCards.push(cardElement);
				} else {
					// Hide duplicate cards within the same department
					cardElement.classList.add('hidden');
					cardElement.style.display = 'none';
				}
			} else {
				cardElement.classList.add('hidden');
				cardElement.style.display = 'none';
			}
		});
		
		// Also process cards with data-faculty-card attribute that might not be in the map
		const allCardsWithDataDepartment = document.querySelectorAll('[data-faculty-card][data-department]');
		allCardsWithDataDepartment.forEach((card) => {
			const cardElement = card as HTMLElement;
			const department = cardElement.dataset.department || '';
			const facultyId = cardElement.dataset.facultyId || '';
			const matchesDepartment = department === activeDepartmentFilter;
			// Check if this card was already processed in the map iteration above
