<script>

const htmlEscapeMap = {
	'&': '&amp;',
	'<': '&lt;',
	'>': '&gt;',
	'"': '&quot;',
	'\'': '&#39;'
};

function escapeHtml(value: unknown): string {
	return (value == null ? '' : String(value)).replace(/[&<>"']/g, (char) => htmlEscapeMap[char as keyof typeof htmlEscapeMap] || char);
}

document.addEventListener('DOMContentLoaded', function() {

    const viewAllBtn = document.getElementById('viewAllBtn');

    const hiddenPrograms = document.querySelectorAll('.program-card.hidden');

    const departmentProgramsModal = document.getElementById('departmentProgramsModal');
    const departmentProgramsModalBody = document.getElementById('departmentProgramsModalBody');
    const departmentProgramsModalSubtitle = document.getElementById('departmentProgramsModalSubtitle');
    const departmentProgramsModalTitle = document.getElementById('departmentProgramsModalTitle');
    const departmentModalCloseButtons = Array.from(document.querySelectorAll('.department-modal-close')) as HTMLButtonElement[];
    const departmentModalOverlay = departmentProgramsModal?.querySelector('.department-modal-overlay') as HTMLElement | null;

    const departmentProgramsData = (() => {
        const rawMap = (window as any).departmentProgramsMap;
        if (!rawMap) return {};
        try {
            const parsed = JSON.parse(rawMap);
            return parsed && typeof parsed === 'object' ? parsed : {};
        } catch (error) {
            console.error('Error parsing department programs map:', error);
            return {};
        }
    })();

    const fallbackProgramsData = (() => {
        const raw = (window as any).serverProgramsData;
        if (!raw) return [];
        try {
            const parsed = JSON.parse(raw);
            return Array.isArray(parsed) ? parsed : [];
        } catch (error) {
            console.error('Error parsing fallback programs data:', error);
            return [];
        }
    })();

    function normalizeDepartmentValue(value: string) {
        return (value || '')
            .toLowerCase()
            .replace(/&/g, 'and')
            .replace(/\s+/g, ' ')
            .trim();
    }

    const departmentDisplayLookup: Record<string, string> = {
        [normalizeDepartmentValue('Department of Early Childhood and Elementary Education')]: 'Department of Early Childhood and Elementary Education',
        [normalizeDepartmentValue('Department of Educational Leadership and Professional Services')]: 'Department of Educational Leadership and Professional Services',
        [normalizeDepartmentValue('Department of Language Education')]: 'Department of Language Education',
        [normalizeDepartmentValue('Department of Science and Mathematics Education')]: 'Department of Science and Mathematics Education',
        [normalizeDepartmentValue('Department of Physical Education')]: 'Department of Physical Education',
        [normalizeDepartmentValue('College of Education')]: 'College of Education'
    };

    const resolveDepartmentPrograms = (department: string) => {
        const rawSearchLower = (department || '').toLowerCase();
        const normalizedInput = normalizeDepartmentValue(department);
        const entries = Object.entries(departmentProgramsData);

        let matchedEntry = entries.find(([key]) => {
            const normalizedKey = normalizeDepartmentValue(key);
            return normalizedKey === normalizedInput ||
                normalizedKey.includes(normalizedInput) ||
                normalizedInput.includes(normalizedKey);
        });

        if (!matchedEntry && normalizedInput) {
            matchedEntry = entries.find(([, programs]) =>
                Array.isArray(programs) &&
                programs.some((program: any) => {
                    const normalizedProgramDepartment = normalizeDepartmentValue(program?.department || '');
                    return normalizedProgramDepartment === normalizedInput ||
                        normalizedProgramDepartment.includes(normalizedInput) ||
                        normalizedInput.includes(normalizedProgramDepartment);
                })
            );
        }

        if (matchedEntry) {
            const [label, programList] = matchedEntry;
            const programs = Array.isArray(programList) ? programList : [];
            if (programs.length) {
                return { label, programs };
            }
        }

        const fallbackMatches = fallbackProgramsData.filter((program: any) => {
            if (!program) return false;
            const candidates = [
                program.department,
                program.department?.department,
                program.program,
                program.name,
                program.title,
                program.degree,
                program.degree?.degree,
                program.level,
                program.level?.level,
                program.description
            ];
            return candidates.some((value) => {
                if (!value) return false;
                const text = typeof value === 'string'
                    ? value
                    : (value && typeof value === 'object' && 'toString' in value)
                        ? String(value)
                        : '';
                return text.toLowerCase().includes(rawSearchLower);
            });
        });

        if (fallbackMatches.length) {
            const fallbackLabel = fallbackMatches[0]?.department?.department ||
                fallbackMatches[0]?.department ||
                fallbackMatches[0]?.program ||
                fallbackMatches[0]?.name ||
                department;
            return {
                label: fallbackLabel || department,
                programs: fallbackMatches
            };
        }

        return {
            label: department,
            programs: []
        };
    };

    const renderDepartmentPrograms = (department: string) => {
        if (!departmentProgramsModalBody) return department;
        const { label, programs } = resolveDepartmentPrograms(department);

        if (!programs.length) {
            departmentProgramsModalBody.innerHTML = `
                <div class="text-center py-12">
                    <p class="text-gray-600 text-sm md:text-base">
                        No academic program descriptions available for <span class="font-semibold">${escapeHtml(label)}</span> at the moment.
                    </p>
                </div>
            `;
            return label;
        }

        const preparedProgramMarkup = programs.map((program: any) => {
            const safeProgramName = escapeHtml(program?.program || program?.name || '');
            const safeDegree = program?.degree ? escapeHtml(program.degree) : 'N/A';
            const safeLevel = program?.level ? escapeHtml(program.level) : 'N/A';
            const safeDuration = program?.duration ? escapeHtml(program.duration) : 'N/A';
            const safeAccreditation = program?.accreditation ? escapeHtml(program.accreditation) : '';
            const safeFileUrl = program?.fileUrl ? escapeHtml(program.fileUrl) : '';
            const safeDisplayName = escapeHtml(program?.name || program?.program || '');
            const safeDescription = program?.description
                ? `<p class="mt-2 text-xs md:text-sm text-gray-600 leading-relaxed">${escapeHtml(program.description)}</p>`
                : '';
            const accreditationMarkup = safeAccreditation
                ? `<span class="inline-flex items-center px-2.5 py-0.5 rounded-full border border-emerald-200 bg-emerald-50 text-emerald-700 text-xs font-medium">${safeAccreditation}</span>`
                : `<span class="text-gray-400 text-xs">TBA</span>`;
            const actionMarkup = safeFileUrl
                ? `<button type="button" onclick="openPDFViewer('${safeFileUrl}', '${safeDisplayName}')" class="inline-flex items-center gap-2 rounded-full border border-[#61063B]/20 bg-[#61063B]/10 px-4 py-2 text-xs font-semibold text-[#61063B] transition-colors duration-200 hover:bg-[#61063B]/20">
                        View Details
                        <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7-7 7M21 12H3"></path>
                        </svg>
                    </button>`
                : `<span class="text-gray-400 text-xs">No file</span>`;
            const mobileAccreditationBadge = safeAccreditation
                ? `<span class="inline-flex items-center rounded-full border border-emerald-200 bg-emerald-50 px-3 py-1 text-xs font-semibold text-emerald-700">${safeAccreditation}</span>`
                : `<span class="inline-flex items-center rounded-full border border-gray-200 bg-gray-50 px-3 py-1 text-xs font-semibold text-gray-500">TBA</span>`;
            const mobileActionMarkup = safeFileUrl
                ? `<button type="button" onclick="openPDFViewer('${safeFileUrl}', '${safeDisplayName}')" class="mt-4 inline-flex w-full items-center justify-center gap-2 rounded-full bg-[#61063B] px-4 py-2 text-sm font-semibold text-white shadow-sm transition-colors duration-200 hover:bg-[#61063B]/90">
                        View Details
                        <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7-7 7M21 12H3"></path>
                        </svg>
                    </button>`
                : `<span class="mt-4 block text-center text-xs text-gray-400">No file</span>`;

            return {
                tableRow: `
                    <tr class="hover:bg-white/60 transition-colors duration-200">
                        <td class="px-5 py-4 align-top">
                            <div class="font-semibold text-sm md:text-base text-[#61063B]">${safeProgramName}</div>
                            ${safeDescription}
                        </td>
                        <td class="px-5 py-4 align-top text-sm text-gray-700">${safeDegree}</td>
                        <td class="px-5 py-4 align-top text-sm text-gray-700">${safeLevel}</td>
                        <td class="px-5 py-4 align-top text-sm text-gray-700">${safeDuration}</td>
                        <td class="px-5 py-4 align-top">${accreditationMarkup}</td>
                        <td class="px-5 py-4 align-top text-right">${actionMarkup}</td>
                    </tr>
                `,
                mobileCard: `
                    <article class="rounded-2xl border border-gray-200 bg-white/90 p-4 shadow-sm transition-shadow duration-200 hover:shadow-md">
                        <div class="space-y-4">
                            <div>
                                <h3 class="text-base font-semibold text-[#61063B]">${safeProgramName}</h3>
                                ${safeDescription}
                            </div>
                            <dl class="space-y-2 text-sm text-gray-600">
                                <div class="flex items-start justify-between gap-4">
                                    <dt class="font-medium text-gray-500">Degree</dt>
                                    <dd class="text-right text-gray-700">${safeDegree}</dd>
                                </div>
                                <div class="flex items-start justify-between gap-4">
                                    <dt class="font-medium text-gray-500">Level</dt>
                                    <dd class="text-right text-gray-700">${safeLevel}</dd>
                                </div>
                                <div class="flex items-start justify-between gap-4">
                                    <dt class="font-medium text-gray-500">Duration</dt>
                                    <dd class="text-right text-gray-700">${safeDuration}</dd>
                                </div>
                                <div class="flex items-start justify-between gap-4">
                                    <dt class="font-medium text-gray-500">Accreditation</dt>
                                    <dd class="text-right">${mobileAccreditationBadge}</dd>
                                </div>
                            </dl>
                            ${mobileActionMarkup}
                        </div>
                    </article>
                `
            };
        });

        const tableRowsMarkup = preparedProgramMarkup.map((entry) => entry.tableRow).join('');
        const mobileCardsMarkup = preparedProgramMarkup.map((entry) => entry.mobileCard).join('');

        departmentProgramsModalBody.innerHTML = `
            <div class="space-y-5">
                <div class="flex flex-wrap items-center justify-between gap-3">
                    <p class="text-sm text-gray-600">Showing <span class="font-semibold text-[#61063B]">${programs.length}</span> academic program${programs.length === 1 ? '' : 's'}</p>
                </div>
                <div class="hidden sm:block overflow-x-auto rounded-2xl border border-gray-200 bg-white/80 backdrop-blur">
                    <table class="min-w-full divide-y divide-gray-200 text-sm text-gray-700">
                        <thead class="bg-[#61063B]/90 text-white text-xs uppercase tracking-wide">
                            <tr>
                                <th scope="col" class="px-5 py-3 text-left font-semibold">Program Name</th>
                                <th scope="col" class="px-5 py-3 text-left font-semibold">Degree Type</th>
                                <th scope="col" class="px-5 py-3 text-left font-semibold">Level</th>
                                <th scope="col" class="px-5 py-3 text-left font-semibold">Duration</th>
                                <th scope="col" class="px-5 py-3 text-left font-semibold">Accreditation</th>
                                <th scope="col" class="px-5 py-3 text-right font-semibold">Actions</th>
                            </tr>
                        </thead>
                        <tbody class="divide-y divide-gray-200 bg-white">
                            ${tableRowsMarkup}
                        </tbody>
                    </table>
                </div>
                <div class="sm:hidden space-y-4">
                    ${mobileCardsMarkup}
                </div>
            </div>
        `;

        return label;
    };

    const openDepartmentProgramsModal = (department: string) => {
        if (!departmentProgramsModal) return;
        const matchedLabel = renderDepartmentPrograms(department);
        const trimmedRequest = (department || '').trim();
        const normalizedRequest = normalizeDepartmentValue(trimmedRequest);
        const trimmedMatched = (matchedLabel || '').trim();
        const normalizedMatched = normalizeDepartmentValue(trimmedMatched);

        const manualRequestLabel = normalizedRequest ? departmentDisplayLookup[normalizedRequest] : undefined;
        const manualMatchedLabel = normalizedMatched ? departmentDisplayLookup[normalizedMatched] : undefined;

        let resolvedLabel = manualMatchedLabel || manualRequestLabel || trimmedMatched || trimmedRequest;

        if (normalizedRequest && normalizedMatched) {
            const labelsMismatch =
                normalizedMatched !== normalizedRequest &&
                !normalizedMatched.includes(normalizedRequest) &&
                !normalizedRequest.includes(normalizedMatched);
            if (labelsMismatch && !manualMatchedLabel && manualRequestLabel) {
                resolvedLabel = manualRequestLabel;
            }
        }

        if (departmentProgramsModalSubtitle) {
            if (resolvedLabel) {
                departmentProgramsModalSubtitle.textContent = resolvedLabel;
                departmentProgramsModalSubtitle.classList.remove('hidden');
                departmentProgramsModalSubtitle.setAttribute('data-department-label', resolvedLabel);
            } else {
                departmentProgramsModalSubtitle.textContent = '';
                departmentProgramsModalSubtitle.classList.add('hidden');
                departmentProgramsModalSubtitle.removeAttribute('data-department-label');
            }
        }
        if (departmentProgramsModalTitle) {
            departmentProgramsModalTitle.textContent = resolvedLabel || 'Department Academic Programs';
        }
        departmentProgramsModal.classList.remove('hidden');
        departmentProgramsModal.classList.add('flex');
        document.body.classList.add('overflow-hidden');
    };

    const closeDepartmentProgramsModal = () => {
        if (!departmentProgramsModal) return;
        departmentProgramsModal.classList.add('hidden');
        departmentProgramsModal.classList.remove('flex');
        document.body.classList.remove('overflow-hidden');
    };

    (window as any).openDepartmentProgramsModal = openDepartmentProgramsModal;
    (window as any).closeDepartmentProgramsModal = closeDepartmentProgramsModal;

    departmentModalCloseButtons.forEach((button) => {
        button.addEventListener('click', closeDepartmentProgramsModal);
    });

    if (departmentModalOverlay) {
        departmentModalOverlay.addEventListener('click', closeDepartmentProgramsModal);
    }

    document.addEventListener('keydown', (event) => {
        if (event.key === 'Escape' && departmentProgramsModal && !departmentProgramsModal.classList.contains('hidden')) {
            closeDepartmentProgramsModal();
        }
    });

    

    viewAllBtn.addEventListener('click', function() {

        // Toggle all hidden programs

        hiddenPrograms.forEach(program => {

            program.classList.toggle('hidden');

        });

        

        // Change button text based on state

        if (viewAllBtn.textContent.includes('View All')) {

            viewAllBtn.innerHTML = 'Show Less <svg class="ml-2 -mr-1 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 12h14M12 5l7 7-7 7"></path></svg>';

        } else {

            viewAllBtn.innerHTML = 'View All Academic Programs <svg class="ml-2 -mr-1 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7m0 0l-7 7m7-7H3"></path></svg>';

        }

    });

});


// Faculty Modal System
const getRankPrefix = (value) => {
	if (!value) return '';
	const normalized = value.toLowerCase();
	if (normalized.includes('assistant professor')) return 'Assistant Prof.';
	if (normalized.includes('associate professor')) return 'Associate Prof.';
	if (normalized.includes('professor')) return 'Prof.';
	return '';
};

const stripLeadingTitles = (name) => {
	if (!name) return '';
	return name.replace(/^(?:(?:asst|assistant|assoc|associate)\.?\s+prof\.?|prof\.?|professor)\s+/i, '').trim();
};

const expandAbbreviatedPrefix = (name) => {
	if (!name) return '';
	return name
		.replace(/^Asst\.\s+Prof\.\s*/i, 'Assistant Prof. ')
		.replace(/^Asst\.\s+/i, 'Assistant ')
		.replace(/^Assoc\.\s+Prof\.\s*/i, 'Associate Prof. ')
		.replace(/^Assoc\.\s+/i, 'Associate ')
		.replace(/^Prof\.\s*/i, 'Prof. ')
		.trim();
};

const formatDisplayName = ({ name, rank, position }) => {
	if (!name) return '';
	const coreName = stripLeadingTitles(name);
	const rankPrefix = getRankPrefix(rank);
	if (rankPrefix) return `${rankPrefix} ${coreName}`.trim();
	const positionPrefix = getRankPrefix(position);
	if (positionPrefix) return `${positionPrefix} ${coreName}`.trim();
	const expanded = expandAbbreviatedPrefix(name);
	return expanded || name;
};

