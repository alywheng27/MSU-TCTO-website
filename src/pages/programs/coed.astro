			<script>

			// Enhanced dark mode functionality for CAS page

			function applyDarkMode(isDark) {

				if (isDark) {

					document.documentElement.classList.add('dark');

					document.body.classList.add('dark');

					document.documentElement.style.backgroundColor = '#111827'; /* gray-900 */

					document.body.style.backgroundColor = '#111827'; /* gray-900 */

					

					// Apply dark mode to all sections

					const sections = document.querySelectorAll('section');

					sections.forEach(section => {

						section.classList.add('dark-mode-section');

					});

					

					// Apply dark mode to containers

					const containers = document.querySelectorAll('.container');

					containers.forEach(container => {

						container.classList.add('dark-mode-container');

					});

					

					// Apply dark mode to About section specifically - matching navbar colors

					const aboutSection = document.querySelector('section.bg-gradient-to-br') as HTMLElement;

					if (aboutSection) {

						aboutSection.style.background = 'linear-gradient(to bottom right, #1f2937, #111827)'; /* gray-800 to gray-900 */

					}

					

					// Apply dark mode to tab buttons

					const tabButtons = document.querySelectorAll('.tab-button');

					tabButtons.forEach(button => {

						(button as HTMLElement).style.color = '#ffffff';

					});

					

					// Apply dark mode to tab content

					const tabContents = document.querySelectorAll('.tab-content');

					tabContents.forEach(content => {

						const paragraphs = content.querySelectorAll('p');

						const headings = content.querySelectorAll('h4');

						const lists = content.querySelectorAll('li');

						

						paragraphs.forEach(p => (p as HTMLElement).style.color = '#ffffff');

						headings.forEach(h => (h as HTMLElement).style.color = '#ffffff');

						lists.forEach(li => (li as HTMLElement).style.color = '#ffffff');

					});

					

					// Apply dark mode to Contact form cards

					const contactCards = document.querySelectorAll('.max-w-6xl.mx-auto.bg-white, .md\\:w-1\\/2.bg-msu-main-color, .md\\:w-1\\/2.p-12');

					contactCards.forEach(card => {

						(card as HTMLElement).style.backgroundColor = '#1f2937'; /* gray-800 */

					});

					

					// Apply dark mode to Academic Programs cards

					const programCards = document.querySelectorAll('.program-card.bg-white');

					programCards.forEach(card => {

						(card as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(card as HTMLElement).style.border = '1px solid #4b5563'; /* gray-600 */

					});

					

					// Apply dark mode to form inputs

					const formInputs = document.querySelectorAll('input, select, textarea');

					formInputs.forEach(input => {

						(input as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(input as HTMLElement).style.borderColor = '#4b5563'; /* gray-600 */

						(input as HTMLElement).style.color = '#ffffff';

					});

					

					// Apply dark mode to select dropdown specifically

					const selectElements = document.querySelectorAll('select');

					selectElements.forEach(select => {

						(select as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(select as HTMLElement).style.color = '#ffffff';

						(select as HTMLElement).style.borderColor = '#4b5563'; /* gray-600 */

					});

					

					// Apply dark mode to select options

					const selectOptions = document.querySelectorAll('select option');

					selectOptions.forEach(option => {

						(option as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(option as HTMLElement).style.color = '#ffffff';

					});

					

					// Apply dark mode to labels

					const labels = document.querySelectorAll('label');

					labels.forEach(label => {

						(label as HTMLElement).style.color = '#ffffff';

					});

					

					// Apply dark mode to Faculty Spotlight cards

					const facultyCards = document.querySelectorAll('.bg-white.rounded-xl.shadow-lg');

					facultyCards.forEach(card => {

						(card as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(card as HTMLElement).style.border = '1px solid #4b5563'; /* gray-600 */

					});

					

					// Apply dark mode to Research & Innovation cards

					const researchCards = document.querySelectorAll('.bg-white.p-8.rounded-xl.shadow-md');

					researchCards.forEach(card => {

						(card as HTMLElement).style.backgroundColor = '#374151'; /* gray-700 */

						(card as HTMLElement).style.border = '1px solid #4b5563'; /* gray-600 */

					});

					

					// Apply dark mode to Student Life cards

					const studentLifeCards = document.querySelectorAll('.relative.group.overflow-hidden.rounded-xl.shadow-lg');

