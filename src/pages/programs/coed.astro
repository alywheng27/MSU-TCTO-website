		/* Slider Animation Styles */
		.student-life-slider {
			position: relative;
			overflow: hidden;
			border-radius: 1rem;
		}

		.slider-track {
			display: flex;
			transition: transform 0.8s cubic-bezier(0.25, 0.46, 0.45, 0.94);
			will-change: transform;
		}

		.slider-slide {
			flex-shrink: 0;
			transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
			transform-origin: center;
		}

		.slider-slide:hover {
			transform: scale(1.02);
		}

		.slider-dot.active {
			background-color: #fbbf24 !important;
			transform: scale(1.2);
		}

		.slider-dot:hover {
			background-color: #fcd34d !important;
		}

		/* Enhanced slider animations */
		@keyframes slideInFromLeft {
			from {
				opacity: 0;
				transform: translateX(-30px) scale(0.95);
			}
			to {
				opacity: 1;
				transform: translateX(0) scale(1);
			}
		}

		@keyframes slideInFromRight {
			from {
				opacity: 0;
				transform: translateX(30px) scale(0.95);
			}
			to {
				opacity: 1;
				transform: translateX(0) scale(1);
			}
		}

		@keyframes slideFadeIn {
			from {
				opacity: 0;
				transform: scale(0.9);
			}
			to {
				opacity: 1;
				transform: scale(1);
			}
		}

		.slide-enter-left {
			animation: slideInFromLeft 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
		}

		.slide-enter-right {
			animation: slideInFromRight 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
		}

		.slide-fade-in {
			animation: slideFadeIn 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94);
		}

		/* Smooth transition effects for active slides */
		.slider-slide.transitioning {
			opacity: 0.7;
			transform: scale(0.98);
		}

		.slider-slide.active {
			opacity: 1;
			transform: scale(1);
		}

		/* Responsive slider adjustments */
		@media (max-width: 768px) {
			.slider-slide {
				width: 100% !important;
			}
		}

		@media (min-width: 769px) and (max-width: 1024px) {
			.slider-slide {
				width: 50% !important;
			}
		}

		@media (min-width: 1025px) {
			.slider-slide {
				width: 25% !important;
			}
