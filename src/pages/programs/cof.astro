// Open faculty modal from URL hash (#faculty-id) or query (?profile=id)
function openFacultyFromHash() {
	if (typeof window === 'undefined') return;
	const hashMatch = window.location.hash.match(/^#faculty-(.+)$/);
	const queryMatch = typeof URLSearchParams !== 'undefined' && new URLSearchParams(window.location.search).get('profile');
	const facultyId = hashMatch?.[1] || queryMatch;
	if (facultyId) {
		openFacultyModal(facultyId);
		if (queryMatch && !hashMatch && window.history && window.history.replaceState) {
			window.history.replaceState(null, '', window.location.pathname + '#faculty-' + facultyId);
		}
	}
}
document.addEventListener('DOMContentLoaded', function() {
	openFacultyFromHash();
	document.querySelectorAll('button[onclick*="openFacultyModal"]').forEach(function(btn) {
		const match = btn.getAttribute('onclick')?.match(/openFacultyModal\s*\(\s*['"]([^'"]+)['"]\s*\)/);
		if (!match || btn.parentElement?.classList?.contains('flex')) return;
		const facultyId = match[1];
		const wrap = document.createElement('div');
		wrap.className = 'flex items-center gap-3 flex-wrap';
		btn.parentNode.insertBefore(wrap, btn);
		wrap.appendChild(btn);
		const shareBtn = document.createElement('button');
		shareBtn.type = 'button';
		shareBtn.className = 'text-msu-deep-ocean font-medium hover:underline flex items-center gap-1.5 text-sm cursor-pointer share-profile-btn-wrap';
		shareBtn.title = 'Share profile';
		shareBtn.innerHTML = '<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8.684 13.342C8.886 12.938 9 12.482 9 12c0-.482-.114-.938-.316-1.342m0 2.684a3 3 0 110-2.684m0 2.684L6 12m2.684-1.316a3 3 0 105.367 2.684L6 12l2.051-2.051m0 0L6 12l2.684-2.684m0 0a3 3 0 105.367-2.684L6 12"></path></svg> Share';
		shareBtn.onclick = function(e) { e.preventDefault(); shareFacultyProfile(facultyId); };
		wrap.appendChild(shareBtn);
	});
});
