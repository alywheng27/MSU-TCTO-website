function renderFeaturedFacultyCards() {
	const facultyGrid = document.getElementById('facultyGrid');

	const legacyAdditionalFaculty = Array.from(document.querySelectorAll('.additional-faculty')) as HTMLElement[];
	if (legacyAdditionalFaculty.length) {
		legacyAdditionalFaculty.forEach((element) => element.remove());
	}
	if (!facultyGrid) return;
	const firstBanner = facultyGrid.querySelector('.faculty-spotlight-banner');

	featuredFacultyEntries.forEach((entry) => {
		// Support both 'department' (single) and 'departments' (array) for backward compatibility
		const departments = (entry as any).departments || ((entry as any).department ? [(entry as any).department] : []);
		
		departments.forEach((department) => {
			// Check if a card for this faculty member already exists in this department
			const existingCard = Array.from(facultyGrid.querySelectorAll(`[data-featured-faculty="${entry.id}"]`)).find(
				(card) => (card as HTMLElement).dataset.department === department
			);
			if (existingCard) return;

			const imageSrc = entry.image || getFacultyImagePath(entry.name) || '/images/cof/Faculty/msufaculty.png';
			const subtitle = entry.subtitle || '';
			const displayName = entry.name || entry.id;
			const cardMarkup = [
				`<div class="bg-white dark:bg-gray-800 rounded-xl shadow-lg overflow-hidden transition-transform duration-300 hover:-translate-y-2 hidden" data-department="${department}" data-departments="${departments.join(',')}" data-faculty-card data-featured-faculty="${entry.id}">`,
				'	<div class="h-90 overflow-hidden relative">',
				`		<img src="${imageSrc}" alt="${displayName}" class="w-full h-full object-cover lazy-image" loading="lazy">`,
				'		<div class="absolute bottom-0 left-0 right-0 bg-gradient-to-t from-black/70 to-transparent p-4">',
				`			<h3 class="text-white font-bold text-xl">${displayName}</h3>`,
				`			<p class="text-gray-300 text-sm">${subtitle}</p>`,
				'		</div>',
				'	</div>',
				'	<div class="p-6 space-y-4">',
				'		<div class="flex items-center">',
				'			<svg class="w-5 h-5 text-msu-deep-ocean dark:text-yellow-300 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">',
				'				<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>',
				'			</svg>',
				`			<span class="text-sm text-msu-main-color dark:text-yellow-200">${entry.highlight}</span>`,
				'		</div>',
				`		<p class="text-msu-main-color dark:text-gray-100 leading-relaxed">${entry.summary}</p>`,
				`		<button onclick="openFacultyModal('${entry.id}')" class="text-msu-deep-ocean dark:text-yellow-300 font-medium hover:underline flex items-center text-sm cursor-pointer">`,
				'			View Profile',
				'			<svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">',
				'				<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>',
				'			</svg>',
				'		</button>',
				'	</div>',
				'</div>'
			].join('\n');

			const departmentBanner = facultyGrid.querySelector(`.faculty-spotlight-banner[data-department="${department}"]`);
			let inserted: HTMLElement | null = null;
			if (departmentBanner) {
				departmentBanner.insertAdjacentHTML('afterend', cardMarkup);
				inserted = departmentBanner.nextElementSibling as HTMLElement | null;
			} else if (firstBanner) {
				firstBanner.insertAdjacentHTML('beforebegin', cardMarkup);
				inserted = firstBanner.previousElementSibling as HTMLElement | null;
			} else {
				facultyGrid.insertAdjacentHTML('beforeend', cardMarkup);
				inserted = facultyGrid.lastElementChild as HTMLElement | null;
			}
			if (inserted) {
				inserted.dataset.featuredFaculty = entry.id;
				inserted.dataset.department = department;
				inserted.dataset.departments = departments.join(',');
				// Ensure card is hidden initially
				inserted.classList.add('hidden');
				inserted.style.display = 'none';
			}
		});
	});
	
	// Ensure all faculty cards are hidden after rendering
	const allCards = facultyGrid.querySelectorAll('[data-faculty-card]') as NodeListOf<HTMLElement>;
	allCards.forEach((card) => {
		card.classList.add('hidden');
		card.style.display = 'none';
	});
}

