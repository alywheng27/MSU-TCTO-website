// Share faculty profile
const SHARE_COLLEGE_NAME = 'College of Fisheries';
function shareFacultyProfile(facultyId) {
	// Use ?profile=id so Messenger/Facebook/Twitter crawlers get og:image (faculty photo)
	const url = typeof window !== 'undefined' ? `${window.location.origin}${window.location.pathname}?profile=${facultyId}` : '';
	const title = `Faculty Profile - MSU-TCTO ${SHARE_COLLEGE_NAME}`;
	const text = 'View this faculty profile at MSU-TCTO.';
	if (typeof navigator !== 'undefined' && navigator.share) {
		navigator.share({ title, text, url }).catch(() => copyShareLink(url));
	} else {
		copyShareLink(url);
	}
}
function copyShareLink(url) {
	if (typeof navigator === 'undefined' || !navigator.clipboard) return;
	navigator.clipboard.writeText(url).then(() => {
		const toast = document.createElement('div');
		toast.className = 'fixed bottom-4 left-1/2 -translate-x-1/2 bg-msu-maroon text-white px-4 py-2 rounded-lg shadow-lg text-sm font-medium z-[9999] animate-fade-in';
		toast.textContent = 'Link copied! Share it with anyone.';
		document.body.appendChild(toast);
		setTimeout(() => toast.remove(), 2500);
	});
}

