import { getCOFPrograms } from '../../api/api.jsx';





const currentPath = Astro.url.pathname;
const site = 'https://msutcto.edu.ph';
const FACULTY_SHARE_MAP: Record<string, { name: string; image: string }> = {
	jaroAjik: { name: 'Prof. Jaro O. Ajik', image: '/images/cof/Faculty/Jaro O. Ajik.png' },
	wahayminJamil: { name: 'Asst. Prof. Wahaymin M. Jamil', image: '/images/cof/Faculty/Wahaymin M. Jamil.png' },
	merilynAmlani: { name: 'Asst. Prof. Merilyn Q. Amlani', image: '/images/cof/Faculty/Merilyn Q. Amlani.png' },
	albarisTahiluddin: { name: 'Assistant Prof. Albaris B. Tahiluddin', image: '/images/cof/Faculty/Albaris B. Tahiluddin.png' },
	jurminSarri: { name: 'Asst. Prof. Jurmin H. Sarri', image: '/images/cof/Faculty/Jurmin H. Sarri.png' },
	mariaLizaToringFarquerabao: { name: 'Maria Liza B. Toring-Farquerabao', image: '/images/cof/Faculty/Maria Liza B. Toring-Farquerabao.png' },
	ainulyakinImlani: { name: 'Prof. Ainulyakin H. Imlani', image: '/images/cof/Faculty/Ainulyakin H. Imlani.png' },
	yashierJumah: { name: 'Prof. Yashier U. Jumah', image: '/images/cof/Faculty/Yashier U. Jumah.png' },
	rosalindaShariff: { name: 'Assoc. Prof. Rosalinda P. Shariff', image: '/images/cof/Faculty/Rosalinda P. Shariff.png' },
	kingpuAjik: { name: 'Assoc. Prof. Kingpu O. Ajik', image: '/images/cof/Faculty/Kingpu O. Ajik.png' }
};
const profileId = Astro.url.searchParams.get('profile');
const facultyShare = profileId && FACULTY_SHARE_MAP[profileId] ? FACULTY_SHARE_MAP[profileId] : null;
const ogImage = facultyShare ? site + facultyShare.image : undefined;
const ogTitle = facultyShare ? `${facultyShare.name} - MSU-TCTO College of Fisheries` : undefined;
const ogDescription = facultyShare ? `View ${facultyShare.name}'s profile at MSU-TCTO COF.` : undefined;
const ogUrl = profileId ? `${site}/programs/cof?profile=${profileId}` : undefined;

// Fetch COF programs data from Sanity
type ProgramDataItem = {
  _id: string;
  program: string;
  description: string;
  degree: string;
  level: string;
  duration: string;
  accreditation: string;
  fileUrl: string | null;
  name: string;
  department: string;
};

let programsData: ProgramDataItem[] = [];
let filterOptions = {
  degrees: [] as string[],
  levels: [] as string[],
  departments: [] as string[]
};

// Helper function to extract text from Sanity block content
function extractTextFromBlocks(blocks: any[]): string {
  if (!blocks || !Array.isArray(blocks)) return '';
  
  return blocks
    .map(block => {
      if (block._type === 'block' && block.children) {
        return block.children
          .map((child: any) => child.text || '')
          .join('');
      }
      return '';
    })
    .join(' ')
    .trim();
}

try {
  const rawProgramsData = await getCOFPrograms();
  
  // Transform Sanity data to match frontend expectations
  programsData = rawProgramsData.map(program => ({
    _id: program._id,
    program: program.title,
    description: extractTextFromBlocks(program.description) || '',
    degree: program.degree?.degree || '',
    level: program.level?.level || '',
    duration: program.duration?.duration || '',
    accreditation: program.accreditation || '',
    fileUrl: program.file?.asset?.url || null,
    name: program.title,
    department: program.department?.department || ''
  }));
  
  // Extract unique filter options
  const degreeSet = new Set<string>();
  const levelSet = new Set<string>();
  const departmentSet = new Set<string>();
  
  programsData.forEach(program => {
    if (program.degree) degreeSet.add(program.degree);
    if (program.level) levelSet.add(program.level);
    if (program.department) departmentSet.add(program.department);
  });
  
  // Convert Sets to Arrays for easier use
  filterOptions.degrees = Array.from(degreeSet);
  filterOptions.levels = Array.from(levelSet);
  filterOptions.departments = Array.from(departmentSet);
  
  console.log('COF Programs loaded:', programsData.length);
  console.log('Filter options:', filterOptions);
  console.log('Sample program:', programsData[0]);
  console.log('Sample program description:', programsData[0]?.description);
  console.log('Raw description data:', rawProgramsData[0]?.description);
} catch (error) {
  console.error('Error fetching COF programs data:', error);
  // Fallback to empty array if fetch fails
  programsData = [];
}

const targetDepartments = [
  'Post-Harvest Fisheries Department',
  'Food Science and Technology Department',
  'Capture Fisheries Department',
  'Aquaculture Department'
];

const normalizeDepartmentName = (value: string) =>
  (value || '')
    .toLowerCase()
    .replace(/&/g, 'and')
    .replace(/\s+/g, ' ')
    .trim();

const createDepartmentKey = (value: string) => {
  const normalized = normalizeDepartmentName(value);
  if (!normalized) return '';
  const cleaned = normalized.replace(/[^a-z0-9\s-]/g, ' ');
  const tokens = cleaned
    .split(/\s+/)
    .filter(Boolean)
    .filter((token) => !['department', 'college', 'of', 'the'].includes(token));
  if (!tokens.length) return normalized;
  return tokens.sort().join(' ');
};

const compareDepartment = (programDepartment: string, targetDepartment: string) => {
  const normalizedProgramDepartment = normalizeDepartmentName(programDepartment);
  const normalizedTarget = normalizeDepartmentName(targetDepartment);
  if (!normalizedTarget) {
    return false;
  }
  if (normalizedProgramDepartment === normalizedTarget) {
    return true;
  }
  if (
    normalizedProgramDepartment.includes(normalizedTarget) ||
    normalizedTarget.includes(normalizedProgramDepartment)
  ) {
    return true;
  }

  const programKey = createDepartmentKey(programDepartment);
  const targetKey = createDepartmentKey(targetDepartment);
  if (programKey && targetKey && programKey === targetKey) {
    return true;
  }

  return false;
};

const canonicalDepartments = targetDepartments.map((department) => ({
  label: department,
  key: createDepartmentKey(department),
  normalized: normalizeDepartmentName(department)
}));

const departmentProgramsMap = canonicalDepartments.reduce<Record<string, ProgramDataItem[]>>((acc, entry) => {
  acc[entry.label] = programsData.filter((program) => compareDepartment(program.department, entry.label));
  return acc;
}, {});

const unmatchedDepartments = filterOptions.departments.filter((department) => {
  const departmentKey = createDepartmentKey(department);
  const normalizedDepartment = normalizeDepartmentName(department);
  return !canonicalDepartments.some((entry) => {
    if (
      entry.key &&
      departmentKey &&
      entry.key === departmentKey
    ) {
      return true;
    }
    if (!normalizedDepartment) {
      return false;
    }
    return (
      entry.normalized === normalizedDepartment ||
      entry.normalized.includes(normalizedDepartment) ||
      normalizedDepartment.includes(entry.normalized)
    );
  });
});

unmatchedDepartments.forEach((department) => {
  departmentProgramsMap[department] = programsData.filter((program) =>
    compareDepartment(program.department, department)
  );
});

console.log('Department programs map created for departments:', Object.keys(departmentProgramsMap));

