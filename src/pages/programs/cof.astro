			const department = banner.getAttribute('data-department') || '';
			const programsContainer = banner.querySelector('.faculty-banner-programs') as HTMLElement | null;
			if (!programsContainer) return;

			const defaultDisplay = programsContainer.dataset.defaultDisplay || (programsContainer.classList.contains('grid') ? 'grid' : 'block');
			if (!programsContainer.dataset.defaultDisplay) {
				programsContainer.dataset.defaultDisplay = defaultDisplay;
			}

			if (!activeDepartmentFilter || department !== activeDepartmentFilter) {
				programsContainer.innerHTML = '';
				programsContainer.classList.add('hidden');
				programsContainer.style.display = 'none';
				return;
			}

			renderProgramsForDepartment(department, programsContainer);
		});
	}

	function applyDepartmentFilter() {
		const departmentCards = [] as HTMLElement[];
		if (!activeDepartmentFilter) {
			hideDepartmentNotice();
			// Hide all faculty cards - query directly from DOM to ensure all cards are handled
			const allCards = facultyGrid.querySelectorAll('[data-faculty-card]') as NodeListOf<HTMLElement>;
			allCards.forEach((card) => {
				card.classList.add('hidden');
				card.style.display = 'none';
			});
			// Also hide cards in the map
			facultyCardMap.forEach((card) => {
				const element = card as HTMLElement;
				element.classList.add('hidden');
				element.style.display = 'none';
			});
			hideAdditionalFacultyCards();
			updateBannerContent([]);
			resetProgramsDisplay();
			return;
		}

		clearActiveHighlight();

		let visibleCount = 0;
		// Query all faculty cards directly from DOM to ensure dynamically inserted cards are included
		const allCards = facultyGrid.querySelectorAll('[data-faculty-card]') as NodeListOf<HTMLElement>;
		allCards.forEach((cardElement) => {
			const department = cardElement.dataset.department || '';
			// Only show cards that match the specific department this card was created for
			// This ensures cards assigned to multiple departments only show for their specific department section
			const matchesDepartment = department === activeDepartmentFilter;
			if (matchesDepartment) {
				cardElement.classList.remove('hidden');
				cardElement.style.display = '';
				visibleCount += 1;
				departmentCards.push(cardElement);
			} else {
				cardElement.classList.add('hidden');
				cardElement.style.display = 'none';
			}
		});

		if (showMoreBtn) {
			showMoreBtn.classList.add('hidden');
		}
		if (facultySearchResults) {
			facultySearchResults.classList.add('hidden');
		}
		if (facultyResultsCount) {
			facultyResultsCount.innerHTML = '';
		}
		if (facultyResultsList) {
			facultyResultsList.innerHTML = '';
		}
		if (clearFacultySearchBtn) {
			clearFacultySearchBtn.classList.add('hidden');
		}

		const staticBanner = document.querySelector(`.faculty-department-banner[data-department="${activeDepartmentFilter}"]`);
		const staticBannerContent = staticBanner?.querySelector('.faculty-banner-content[data-static-content="true"]') as HTMLElement | null;
		const staticMemberCount = staticBannerContent
			? parseInt(staticBannerContent.dataset.memberCount || '0', 10) ||
				staticBannerContent.querySelectorAll('[data-banner-member]').length
			: 0;

		if (visibleCount === 0) {
			if (staticBannerContent) {
				showDepartmentNotice(activeDepartmentFilter, staticMemberCount || 0);
				updateBannerContent([]);
				updateProgramsDisplay();
				return;
			}
			if (facultyDepartmentNotice) {
				facultyDepartmentNotice.innerHTML = `No faculty members found for <span class="font-semibold">${escapeHtml(activeDepartmentFilter)}</span>`;
				facultyDepartmentNotice.classList.remove('hidden');
			}
			return;
		}

		showDepartmentNotice(activeDepartmentFilter, visibleCount);
		updateBannerContent(departmentCards);
		updateProgramsDisplay();

		const firstVisibleCard = Array.from(facultyGrid.querySelectorAll<HTMLElement>('[data-faculty-id]')).find((card) => card.dataset.department === activeDepartmentFilter);
		if (firstVisibleCard) {
			firstVisibleCard.scrollIntoView({ behavior: 'smooth', block: 'start' });
		}
	}

	updateDepartmentButtonsState();
	hideDepartmentNotice();
	updateProgramsDisplay();

	if (departmentFacultyButtons.length) {
		departmentFacultyButtons.forEach((button) => {
			button.addEventListener('click', (event) => {
				event.preventDefault();
				const department = button.dataset.department;
				if (!department) return;
				const isActive = activeDepartmentFilter === department;
				activeDepartmentFilter = isActive ? null : department;
				if (facultySearchInput) {
					(facultySearchInput as HTMLInputElement).value = '';
				}
				filterFaculty('');
				applyDepartmentFilter();
				updateDepartmentButtonsState();
				resetProgramsDisplay();
				if (typeof closeDepartmentProgramsModalFn === 'function') {
					closeDepartmentProgramsModalFn();
				}
			});
		});
	}

	if (departmentProgramButtons.length) {
		departmentProgramButtons.forEach((button) => {
			button.addEventListener('click', (event) => {
				event.preventDefault();
				const rawDepartment = button.dataset.department || '';
				const searchTerm = button.dataset.departmentSearch || rawDepartment || button.textContent || '';
				const department = searchTerm.trim();
				if (!department) return;
				if (typeof openDepartmentProgramsModalFn === 'function') {
					openDepartmentProgramsModalFn(department);
				}
			});
		});
