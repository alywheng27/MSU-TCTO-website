                                shareCalendarBtn.classList.add('bg-gradient-to-r', 'from-[#61063B]', 'to-[#7a0a4f]');
                            }, 2000);
                        } catch (clipboardError) {
                            // If clipboard also fails, show alert
                            alert('Link: ' + shareUrl + '\n\nPlease copy this link manually.');
                        }
                    }
                }
            });
        }
        
        // Auto-open calendar modal if URL parameter is present
        const urlParams = new URLSearchParams(window.location.search);
        if (urlParams.get('calendar') === 'open') {
            // Small delay to ensure page is fully loaded
            setTimeout(() => {
                openAcademicCalendarModal();
                // Clean up URL parameter without page reload
                const newUrl = new URL(window.location.href);
                newUrl.searchParams.delete('calendar');
                window.history.replaceState({}, '', newUrl.toString());
            }, 300);
        }
        
        // Global Search Functionality
        const calendarSearchInput = document.getElementById('calendarSearchInput') as HTMLInputElement;
        const clearSearchBtn = document.getElementById('clearSearchBtn') as HTMLButtonElement;
        const searchResultsCount = document.getElementById('searchResultsCount') as HTMLElement;
        const calendarContent = document.getElementById('academicCalendarContent');
        
        if (calendarSearchInput && calendarContent) {
            // Highlight matching text
            const highlightText = (text: string, searchTerm: string): string => {
                if (!searchTerm) return text;
                const regex = new RegExp(`(${searchTerm.replace(/[.*+?^${}()|[\]\\]/g, '\\$&')})`, 'gi');
                return text.replace(regex, '<mark class="bg-yellow-200 dark:bg-yellow-800 px-1 rounded">$1</mark>');
            };
            
            // Search function
            const performSearch = (searchTerm: string) => {
                if (!calendarContent) return;
                
                const searchLower = searchTerm.toLowerCase().trim();
                let totalMatches = 0;
                let visibleSections = 0;
                
                // Get all sections
                const sections = calendarContent.querySelectorAll('div[class*="bg-gradient"]');
                
                sections.forEach((section) => {
                    const sectionEl = section as HTMLElement;
                    const h4 = sectionEl.querySelector('h4');
                    const table = sectionEl.querySelector('table');
                    let sectionHasMatch = false;
                    
                    if (table) {
                        const rows = table.querySelectorAll('tbody tr');
                        let visibleRows = 0;
                        
                        rows.forEach((row) => {
                            const rowEl = row as HTMLElement;
                            const cells = rowEl.querySelectorAll('td');
                            let rowHasMatch = false;
                            let rowText = '';
                            
                            cells.forEach((cell) => {
                                const cellText = cell.textContent || '';
                                rowText += cellText + ' ';
                                if (cellText.toLowerCase().includes(searchLower)) {
                                    rowHasMatch = true;
                                }
                            });
                            
                            if (searchLower === '') {
                                // Show all rows when search is empty
                                rowEl.style.display = '';
                                visibleRows++;
                            } else if (rowHasMatch) {
                                // Show matching rows
                                rowEl.style.display = '';
                                visibleRows++;
                                totalMatches++;
                                
                                // Highlight matching text in cells
                                cells.forEach((cell) => {
                                    const originalText = cell.textContent || '';
                                    if (originalText.toLowerCase().includes(searchLower)) {
                                        cell.innerHTML = highlightText(originalText, searchTerm);
                                    }
                                });
                            } else {
                                // Hide non-matching rows
                                rowEl.style.display = 'none';
                            }
                        });
                        
                        // Show/hide section based on matches
                        if (searchLower === '' || visibleRows > 0) {
                            sectionEl.style.display = '';
                            if (visibleRows > 0) visibleSections++;
                        } else {
                            sectionEl.style.display = 'none';
                        }
                        
                        sectionHasMatch = visibleRows > 0;
                    }
                    
                    // Check if section title matches
                    if (h4 && searchLower !== '') {
                        const titleText = h4.textContent || '';
                        if (titleText.toLowerCase().includes(searchLower)) {
                            sectionHasMatch = true;
                            if (sectionEl.style.display === 'none') {
                                sectionEl.style.display = '';
                                visibleSections++;
                            }
                            h4.innerHTML = highlightText(titleText, searchTerm);
                        }
                    }
                });
                
                // Update results count
                if (searchLower === '') {
                    searchResultsCount.classList.add('hidden');
                    clearSearchBtn.classList.add('hidden');
                } else {
                    searchResultsCount.classList.remove('hidden');
                    if (totalMatches > 0) {
                        searchResultsCount.textContent = `Found ${totalMatches} result${totalMatches !== 1 ? 's' : ''} in ${visibleSections} section${visibleSections !== 1 ? 's' : ''}`;
                        searchResultsCount.className = 'text-center mt-2 text-sm text-green-600 dark:text-green-400';
                    } else {
                        searchResultsCount.textContent = 'No results found';
                        searchResultsCount.className = 'text-center mt-2 text-sm text-red-600 dark:text-red-400';
                    }
                    clearSearchBtn.classList.remove('hidden');
                }
            };
            
            // Search input event listener
            calendarSearchInput.addEventListener('input', function() {
                const searchTerm = this.value;
                performSearch(searchTerm);
            });
            
            // Clear search button
            if (clearSearchBtn) {
                clearSearchBtn.addEventListener('click', function() {
                    calendarSearchInput.value = '';
                    performSearch('');
                    calendarSearchInput.focus();
                });
            }
            
            // Clear highlights when search is cleared
            const clearHighlights = () => {
                if (!calendarContent) return;
                const marks = calendarContent.querySelectorAll('mark');
                marks.forEach((mark) => {
                    const parent = mark.parentNode;
                    if (parent) {
                        parent.replaceChild(document.createTextNode(mark.textContent || ''), mark);
                        parent.normalize();
                    }
                });
            };
            
            // Reset search when modal is closed
            const resetSearch = () => {
                calendarSearchInput.value = '';
                performSearch('');
                clearHighlights();
            };
            
            // Reset search when modal closes - observe modal visibility
            if (academicCalendarModal) {
                const observer = new MutationObserver((mutations) => {
                    mutations.forEach((mutation) => {
                        if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
                            const target = mutation.target as HTMLElement;
                            if (target.classList.contains('hidden')) {
                                // Modal was closed, reset search
                                resetSearch();
                            }
                        }
                    });
                });
                
                observer.observe(academicCalendarModal, {
                    attributes: true,
                    attributeFilter: ['class']
                });
            }
        }
    }
});
</script>

